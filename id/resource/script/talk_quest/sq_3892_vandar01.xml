   <talk_paragraph index="n2675_hongryun_ticketer-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2675_hongryun_ticketer-1-go_rdmes" mid="12" select_type="0"><![CDATA[<html><FONT color=#662500>Yes. I'd like to leave now.</FONT></html>]]></talk_answer>
      <talk_answer link_index="n2675_hongryun_ticketer-1-a" mid="13" select_type="0"><![CDATA[<html><FONT color=#662500>I can't right now. I'll be back later.</FONT></html>]]></talk_answer>
      <talk_question mid="11"><![CDATA[<html>$Ticket Agent Itahyang$<BR>Will you be setting out immediately?</html>]]></talk_question>
   </talk_paragraph>
