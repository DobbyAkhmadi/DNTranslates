   <talk_paragraph index="n2675_hongryun_ticketer-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2675_hongryun_ticketer-accepting-a" mid="6" select_type="0"><![CDATA[<html><FONT color=#662500>Hmm. What is it?</FONT></html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Ticket Agent Itahyang$<BR>Hello. A red mole of the desert was looking for you.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2675_hongryun_ticketer-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2675_hongryun_ticketer-accepting-b" mid="8" select_type="0"><![CDATA[<html><FONT color=#662500>I see. I'll be on my way, then.</FONT></html>]]></talk_answer>
      <talk_question mid="7"><![CDATA[<html>$Ticket Agent Itahyang$<BR>I have no idea. But I think you'd better stop by there if you have time.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2675_hongryun_ticketer-accepting-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
