      <talk_answer link_index="n519_cian-4-b" mid="1105" select_type="2" />
      <talk_question mid="1104"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Kamu juga baru mengetahui identitas Gerraint?<BR>Kejutan yang besar sekali.</FONT></html>]]></talk_question>
