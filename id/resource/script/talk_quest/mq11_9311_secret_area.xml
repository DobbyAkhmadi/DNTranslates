   <talk_paragraph index="n217_argenta-3-l" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n217_argenta-3-m" mid="1093" select_type="0"><![CDATA[<html>Kamu mau pergi kemana sekarang?</html>]]></talk_answer>
