   <talk_paragraph index="n1205_lunaria-7-i" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1205_lunaria-8" mid="203" select_type="2" />
      <talk_question mid="52"><![CDATA[<html>$Lunaria$<BR><BR><BR>[Lalu aku]....</html>]]></talk_question>
