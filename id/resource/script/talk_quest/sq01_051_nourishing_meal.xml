   <talk_paragraph index="n003_trader_dolis-1-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n003_trader_dolis-1-d" mid="26" select_type="0"><![CDATA[<html>Bagaimana jika kamu ubah peraturan itu?</html>]]></talk_answer>
