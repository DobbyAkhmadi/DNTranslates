   <talk_paragraph index="n2253_nophee-1-b" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Nophee.dds" center_image="">
      <talk_answer link_index="n2253_nophee-1-c" mid="12" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Yup, kita akan menggunakan bantuannya tapi aku juga ingin menanyakan identitas dia yang sebenarnya.</FONT></html>]]></talk_question>
