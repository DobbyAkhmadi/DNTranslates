      <talk_answer link_index="n2253_nophee-1-e" mid="16" select_type="2" />
      <talk_question mid="15"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Sekarang sih dia sudah tidak apa-apa...</FONT></html>]]></talk_question>
