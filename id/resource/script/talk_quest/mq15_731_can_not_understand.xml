      <talk_answer link_index="n724_karahan-2-!next" mid="118" select_type="2" />
      <talk_question mid="117"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Aku mengerti! Udah buat aja obatnya!</FONT></html>]]></talk_question>
