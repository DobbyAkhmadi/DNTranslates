   <talk_paragraph index="n2007_sterope-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2007_sterope-1-a" mid="16" select_type="0"><![CDATA[<html>Kairon Island…</html>]]></talk_answer>
      <talk_question mid="15"><![CDATA[<html>$Merchant Stefani$<BR>Tapi aku masih mendengar tentang rumor dari Kairon Island.</html>]]></talk_question>
