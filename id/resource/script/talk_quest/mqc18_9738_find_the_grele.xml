   <talk_paragraph index="n1816_vernicka-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Left_vernicka.dds" center_image="">
      <talk_answer link_index="n1816_vernicka-1-a" mid="46" select_type="0"><![CDATA[<html>Ya, aku akan pergi ke Southern Territory.</html>]]></talk_answer>
