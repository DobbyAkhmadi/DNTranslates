   <talk_paragraph index="n1816_vernicka-1-a" image="" itemindex="" portrait_left="" portrait_right="Npc_Left_vernicka.dds" center_image="">
      <talk_answer link_index="n1816_vernicka-1-c" mid="48" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>$Vernika the Merkha Governor$<BR>kau telah membuat keputusan tepat. Tetapi tuan <FONT color=#a82424>Norf</FONT> meminta tolong akan satu hal lagi.</html>]]></talk_question>
