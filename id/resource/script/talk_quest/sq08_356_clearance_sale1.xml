   <talk_paragraph index="n026_trader_may-2-e" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n026_trader_may-2-f" mid="42" select_type="0"><![CDATA[<html>Balon? </html>]]></talk_answer>
      <talk_question mid="41"><![CDATA[<html>$Merchant May$<BR>Hmm...Itu dia...ayo pasang iklan dengan balon?</html>]]></talk_question>
