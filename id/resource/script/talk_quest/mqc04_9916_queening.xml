   <talk_paragraph index="n2978_hwaran-accepting" image="" itemindex="" portrait_left="Npc_Left_hwaran.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2978_hwaran-accepting-acceptted" mid="6" select_type="2" />
      <talk_question mid="5"><![CDATA[<html>$Commander Hwaran$<BR>Haha... No reward would be too much for you. I'll be sure to do so.</html>]]></talk_question>
