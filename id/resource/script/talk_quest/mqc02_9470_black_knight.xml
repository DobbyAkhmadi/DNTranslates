   <talk_paragraph index="n1690_eltia-6" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1690_eltia-6-a" mid="139" select_type="2" />
