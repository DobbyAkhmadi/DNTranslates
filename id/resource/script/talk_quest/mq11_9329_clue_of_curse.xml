      <talk_answer link_index="n533_triana-3-f" mid="2918" select_type="0"><![CDATA[<html>Aku tidak tahu.</html>]]></talk_answer>
      <talk_answer link_index="n533_triana-3-f" mid="2919" select_type="0"><![CDATA[<html>Aku sedang berpikir apakah aku harus.</html>]]></talk_answer>
      <talk_answer link_index="n533_triana-3-f" mid="2920" select_type="0"><![CDATA[<html>Itu juga baik.</html>]]></talk_answer>
