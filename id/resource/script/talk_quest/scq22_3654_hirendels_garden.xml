   <talk_paragraph index="n1871_trader_april-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1871_trader_april-1-a" mid="44" select_type="0"><![CDATA[<html>Can I trust you? <FONT color=#7e12d2>(*Gulp*)</FONT></html>]]></talk_answer>
