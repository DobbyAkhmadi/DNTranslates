   <talk_paragraph index="n701_oldkarakule-2" image="QW_54.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-2-" mid="106" select_type="5" />
      <talk_question mid="18"><![CDATA[<html>$Karacule Fairystar$<BR>The Natives girl was spotted wandering around the <FONT color=#a82424>Dragon Follower's Base.</FONT> Take <FONT color=#a82424>Sidel</FONT> and find her.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-2" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2-a" mid="107" select_type="2" />
      <talk_question mid="19"><![CDATA[<html>$Sidel$<BR>Wow! I can't believe how different you look! It hasn't been that long since I saw you, has it?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-2-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2-b" mid="109" select_type="2" />
      <talk_question mid="108"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>And you look exactly the same!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-2-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2-c" mid="110" select_type="2" />
      <talk_question mid="21"><![CDATA[<html>$Sidel${action:Talk}<BR>It's not my fault you're too dense to notice how I've changed. ...How was that? The Master has been putting me through special sorceress training. She says I need to work on my cattiness. Though she's been giving me weird looks...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-2-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2-d" mid="112" select_type="2" />
      <talk_question mid="111"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>What kind of looks?</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-2-d" image="QN_Npc_Charty.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2-e" mid="113" select_type="2" />
      <talk_question mid="23"><![CDATA[<html>$Sidel$<BR>It's nothing. Anyway, we've got to find <FONT color=#a82424>Charti.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-2-e" image="QN_Npc_Charty.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-3" mid="115" select_type="2" />
      <talk_question mid="114"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>You're right. </FONT><FONT color=#a82424>Charti</FONT><FONT color=#662500> needs us.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-3" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-3-" mid="116" select_type="5" />
      <talk_question mid="25"><![CDATA[<html>$Sidel$<BR>Just you wait, <FONT color=#7e12d2>Charti!</FONT> We'll save you yet!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n495_sidel-3" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n495_sidel-3-a" mid="117" select_type="2" />
      <talk_question mid="27"><![CDATA[<html>$Sidel$<BR><FONT color=#7e12d2>Charti! Charti,</FONT> wake up! I'm here to rescue you!<BR><FONT color=#7e12d2>(Sidel holds Charti in his arms and cries.)</FONT></html>]]></talk_question>
