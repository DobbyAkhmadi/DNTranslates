   <talk_paragraph index="n779_jasmin-2-i" image="QN_Npc_Academic_jasmine.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n779_jasmin-2-j" mid="47" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="35"><![CDATA[<html>$Tinkerer Jasmine$<BR>Aku tak mau memikirkan kegagalan.<BR>Namun, jika memang terjadi...<BR>kita harus menggunakan cara terakhir.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n779_jasmin-2-j" image="QN_Npc_Velskud.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n779_jasmin-2-k" mid="48" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="36"><![CDATA[<html>$Velskud$<BR>Cara terakhir?</html>]]></talk_question>
