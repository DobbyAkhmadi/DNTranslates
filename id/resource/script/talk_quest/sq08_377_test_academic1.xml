   <talk_paragraph index="n779_jasmin-2-h" image="QN_Npc_Velskud.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n779_jasmin-2-i" mid="46" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="34"><![CDATA[<html>$Velskud$<BR>Bagaimana jika gagal, apa yang kita lakukan?<BR>Apakah kamu ada rencana lain di pikiranmu?</html>]]></talk_question>
