   <talk_paragraph index="n1067_elder_elf-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1067_elder_elf-accepting-a" mid="6" select_type="0"><![CDATA[<html>Ada apa?</html>]]></talk_answer>
