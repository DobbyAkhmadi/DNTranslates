   <talk_paragraph index="n217_argenta-3-p" image="QN_Npc_Cian.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n217_argenta-3-" mid="202" select_type="5" />
      <talk_question mid="122"><![CDATA[<html>$Argenta$<BR>Lagipula, Aku akan pergi ke Lotus Marsh, bermain dengan <FONT color=#a82424>bocah-bocah itu</FONT> atau apapun itu.<BR>Bye.<BR><FONT color=#7e12d2>(Argenta pergi tanpa melihat ke belakang sekalipun)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n217_argenta-4" image="QN_Npc_Cian.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n217_argenta-4-" mid="203" select_type="5" />
      <talk_question mid="123"><![CDATA[<html>$Argenta$<BR>Lagipula, Aku akan pergi ke Lotus Marsh, bermain dengan <FONT color=#a82424>bocah-bocah itu</FONT> atau apapun itu.<BR>Bye.<BR><FONT color=#7e12d2>(Argenta pergi tanpa melihat ke belakang sekalipun)</FONT></html>]]></talk_question>
