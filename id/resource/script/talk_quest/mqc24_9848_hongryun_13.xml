      <talk_answer link_index="n2722_grele-1-v" mid="91" select_type="2" />
      <talk_question mid="28"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Her nickname when she was young or a pen name…</FONT></html>]]></talk_question>
