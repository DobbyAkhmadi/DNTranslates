   <talk_paragraph index="n714_cleric_yohan-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n714_cleric_yohan-accepting-a" mid="7" select_type="0"><![CDATA[<html>Lalu apa yang ingin kamu lakukan?</html>]]></talk_answer>
