   <talk_paragraph index="n754_cian-1" image="QN_Npc_Geraint_Closeeye.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n754_cian-1-" mid="72" select_type="5" />
