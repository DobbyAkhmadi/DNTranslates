   <talk_paragraph index="n713_soceress_tamara-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n713_soceress_tamara-accepting-b" mid="9" select_type="0"><![CDATA[<html>(Apakah karena meminum air rebusannya?)</html>]]></talk_answer>
