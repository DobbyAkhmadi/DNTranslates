   <talk_paragraph index="n2109_inventor_narlin-1-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2109_inventor_narlin-1-b" mid="12" select_type="0"><![CDATA[<html>Sepertinya menyebalkan.</html>]]></talk_answer>
      <talk_question mid="11"><![CDATA[<html>$Inventor Narlin$<BR>Sepertinya ada sesuatu di Chatedra Confessional yang mengganggunya.<BR>Dia tipe orang yang terbangun di tengah malam disaat galau.</html>]]></talk_question>
