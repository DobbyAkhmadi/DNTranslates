   <talk_paragraph index="n2036_merca_iris-3-k" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_iris.dds" center_image="">
      <talk_answer link_index="n2036_merca_iris-3-l" mid="116" select_type="2" />
      <talk_question mid="50"><![CDATA[<html>$Iris$<BR>Ya! Walaupun aku tidak mengetahui segalanya, di suatu tempat telah terbentuk<FONT color=#a82424> dua buah Nest</FONT>.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2036_merca_iris-3-l" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_iris.dds" center_image="">
      <talk_answer link_index="n2036_merca_iris-3-m" mid="117" select_type="2" />
      <talk_question mid="51"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Tidak mungkin, Rubinart dan Gerraint? Apakah Hiver yang membentuknya?!</FONT></html>]]></talk_question>
