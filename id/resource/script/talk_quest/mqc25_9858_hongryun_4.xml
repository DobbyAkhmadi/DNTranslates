   <talk_paragraph index="n2818_blood_hammer-1-c" image="" itemindex="" portrait_left="Npc_Left_hongryun_senate.dds.dds" portrait_right="Npc_Right_hongryun_pether_mask.dds.dds" center_image="">
      <talk_answer link_index="n2818_blood_hammer-1-d" mid="16" select_type="2" />
      <talk_question mid="15"><![CDATA[<html>$Ultima$<BR>Great army?<BR>They looked like a bunch of sorry gambling addicts to me.</html>]]></talk_question>
