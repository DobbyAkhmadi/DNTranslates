   <talk_paragraph index="n359_trader_merlin-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n359_trader_merlin-accepting-a" mid="7" select_type="0"><![CDATA[<html>Ada apa?</html>]]></talk_answer>
      <talk_question mid="6"><![CDATA[<html>$Blacksmith Merlin$<BR>Selamat datang. Aku adalah Merlin yang datang dari Tel Numara.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n359_trader_merlin-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n359_trader_merlin-accepting-acceptted" mid="9" select_type="0"><![CDATA[<html><FONT color=#086171>Terima Quest</FONT></html>]]></talk_answer>
