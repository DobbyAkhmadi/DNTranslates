   <journal_data recompense="94780" quest_title="세인트헤이븐의 왕성" chapterid="4" quest_level="24" quest_mark_type="1" use_item_list="">
      <journal index="1">
         <contents_image>QN_NPC_Engineer_Hubert.dds</contents_image>
