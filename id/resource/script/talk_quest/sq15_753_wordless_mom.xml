   <talk_paragraph index="n199_darklair_castia-1-b" image="QN_Npc_eclairer.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n199_darklair_castia-2" mid="16" select_type="0"><![CDATA[<html>...Bagaimana aku dapat menemukannya?</html>]]></talk_answer>
