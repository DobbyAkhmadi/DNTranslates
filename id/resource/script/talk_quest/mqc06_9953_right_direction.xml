   <journal_data recompense="99530" quest_title="방향 잡기" chapterid="106" quest_level="95" quest_mark_type="1" use_item_list="">
      <journal index="1">
         <contents_image>QN_Npc_geraint_Kid.dds</contents_image>
         <destination_pos>175,n2067</destination_pos>
         <destination_mode>custom:175@1</destination_mode>
         <statemark_npcid>2067</statemark_npcid>
         <statemark_type>2</statemark_type>
         <trigger_string />
         <maptool_index />
         <title mid="149"><![CDATA[Break Time]]></title>
         <contents mid="150"><![CDATA[<html>Kali ran off to find Geraint. You suddenly find yourself curious again about the identity of that blonde-haired boy. Take a break and talk to the others.<BR>
<BR><FONT color=#086171> Rhadames </FONT> - <FONT color=#a82424> Norfe </FONT></html>]]></contents>
         <destination mid="151"><![CDATA[(Solo) Speak with Norfe in Rhadames]]></destination>
