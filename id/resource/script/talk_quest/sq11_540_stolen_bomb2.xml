   <talk_paragraph index="n042_general_duglars-accepting" image="QS_Default.dds" itemindex="400147" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n042_general_duglars-accepting-a" mid="6" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$General Douglas$<BR>Sangat pasti ini adalah bagian dari kotak kayu yang disimpan di Royal Armory.<BR>Kebakaran tersebut hanya untuk menutupi pencurian bom.</html>]]></talk_question>
   </talk_paragraph>
