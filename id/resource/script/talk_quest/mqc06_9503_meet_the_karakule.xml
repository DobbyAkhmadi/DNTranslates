   <talk_paragraph index="n462_sidel-4-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n462_sidel-4-!next" mid="115" select_type="2" />
