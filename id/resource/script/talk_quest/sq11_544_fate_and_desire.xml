   <talk_paragraph index="n044_archer_master_ishilien-1-g" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n044_archer_master_ishilien-1-h" mid="62" select_type="0"><![CDATA[<html>Tapi aku sangat takut kehilangan sesuatu.</html>]]></talk_answer>
      <talk_question mid="61"><![CDATA[<html>$Master Archer Eithain$<BR>
