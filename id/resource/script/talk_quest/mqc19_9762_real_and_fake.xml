   <talk_paragraph index="n2128_geraint-4-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_geraint_Kid.dds" center_image="">
      <talk_answer link_index="n2128_geraint-4-c" mid="74" select_type="2" />
