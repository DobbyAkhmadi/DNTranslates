   <talk_paragraph index="n1046_slave_david-3" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1046_slave_david-3-a" mid="35" select_type="0"><![CDATA[<html>Tidak ada sesuatu yang spesial.</html>]]></talk_answer>
      <talk_question mid="34"><![CDATA[<html>$David$<BR>Kuharap kamu tidak menipuku kali ini.</html>]]></talk_question>
