   <talk_paragraph index="n1046_slave_david-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1046_slave_david-accepting-b" mid="8" select_type="0"><![CDATA[<html>Apaaa...</html>]]></talk_answer>
      <talk_question mid="7"><![CDATA[<html>$David$<BR>Katanya kamu akan meyakinkan Argenta, dan sekarang kamu tidak menghiraukan permintaanku...<BR><FONT color=#7e12d2>(David menatapmu)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1046_slave_david-accepting-b" image="QN_Npc_Argenta.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1046_slave_david-accepting-c" mid="11" select_type="0"><![CDATA[<html>(Oh tidak. Lari!)</html>]]></talk_answer>
      <talk_question mid="9"><![CDATA[<html>$David$<BR>Ini tidak mungkin terjadi!!<BR>Aku mengabdikan semua yang aku lakukan demi dirinya!<BR>
<FONT color=#7e12d2>(David tetap berteriak kebingunan. Sepertinya dia akan segera mengamuk.)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1046_slave_david-accepting-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1046_slave_david-accepting-acceptted" mid="13" select_type="0"><![CDATA[<html><FONT color=#086171>Terima Quest</FONT> </html>]]></talk_answer>
