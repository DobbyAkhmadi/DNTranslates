   <talk_paragraph index="n045_soceress_master_stella-1-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_SoceressStella.dds" center_image="">
      <talk_answer link_index="n045_soceress_master_stella-1-c" mid="80" select_type="2" />
      <talk_question mid="79"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(It seems strange that no one knew it when a whole army was moving)</FONT></html>]]></talk_question>
