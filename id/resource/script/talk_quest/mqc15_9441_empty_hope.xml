   <talk_paragraph index="n1578_for_memory-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1578_for_memory-accepting-acceptted" mid="6" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(What's happening? Where can I go now?)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1578_for_memory-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1578_for_memory-1-" mid="183" select_type="5" />
      <talk_question mid="7"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(I'll never find the answer if I keep thinking to myself here... I don't know where I'm at or what time period I'm in... This is not good...)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-2-a" mid="89" select_type="2" />
      <talk_question mid="8"><![CDATA[<html>$Gaharam$<BR><FONT color=#7e12d2>(Gaharam looks suprised to see you)</FONT><BR>Are you alright?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2-a" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-2-b" mid="90" select_type="2" />
      <talk_question mid="9"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>?!?! Gaharam?!</FONT><BR><FONT color=#662500>What are you doing here?</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2-b" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-2-c" mid="91" select_type="2" />
      <talk_question mid="10"><![CDATA[<html>$Gaharam$<BR>I heard that you were sighted somewhere near here, so I came to find you.<BR>I'm so glad that you're alive. How are you feeling? How did you escape that place? What did you see in there?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2-c" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-2-d" mid="92" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>One question at a time please! </FONT><BR><FONT color=#662500>I saw....</FONT><BR><FONT color=#662500>(Tell her everything except the future Gaharam)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2-d" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-2-e" mid="93" select_type="2" />
      <talk_question mid="12"><![CDATA[<html>$Gaharam$<BR><FONT color=#7e12d2>(Gaharam turns pale)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2-e" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-2-level_check_1" mid="94" select_type="2" />
      <talk_question mid="13"><![CDATA[<html>${user_nick}$<BR><FONT color=#a82424>Gaharam?</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2-f" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-2-" mid="172" select_type="5" />
      <talk_question mid="170"><![CDATA[<html>$Gaharam$<BR>...Father has finally... chosen that path....<BR>...Please give me some time to collect my thoughts.<BR><BR><FONT color=#7e12d2>*You can proceed when you reach Lv77.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-2-g" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-3" mid="95" select_type="2" />
      <talk_question mid="14"><![CDATA[<html>$Gaharam$<BR>Let's talk somewhere else. Please meet me at the Wailing Wall.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1558_gaharam-3" image="QW_95.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1558_gaharam-3-" mid="96" select_type="5" />
      <talk_question mid="15"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Okay. (Pergilah ke </FONT><FONT color=#a82424>Wailing Wall</FONT><FONT color=#662500>)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="" center_image="">
      <talk_answer link_index="n1577_gaharam-3-a" mid="98" select_type="2" />
      <talk_question mid="16"><![CDATA[<html>$Ancient 1$<BR>Gaharam! Where were you? We were so worried tentang you.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-a" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="NPC_Right_Ancient_minion.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-b" mid="99" select_type="2" />
      <talk_question mid="17"><![CDATA[<html>$Ancient 2$<BR>That weak human is still alive?! Did you save that human, Gaharam?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-b" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-c" mid="100" select_type="2" />
      <talk_question mid="18"><![CDATA[<html>$Gaharam$<BR>Please show some respect. This friend is not as strong as us, but strong enough to live without my protection.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-c" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-d" mid="101" select_type="2" />
      <talk_question mid="19"><![CDATA[<html>$Ancient 3$<BR>It must be luck not strength that saved that human.<BR>
Why would I need to show my respect for such lowly beings...<FONT color=#7e12d2>(The Ancients look unhappy)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-d" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-e" mid="102" select_type="2" />
      <talk_question mid="20"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(They're really mean... I don't want to become like them even if I could have their power)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-e" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-f" mid="103" select_type="2" />
      <talk_question mid="21"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_01.ogg}<BR>Please be courteous. This friend has been a lot of help to us.<BR>I will brief you on what our friend has found out. They finally did it.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-f" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-g" mid="104" select_type="2" />
      <talk_question mid="22"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_02.ogg}<BR>They used poison to connect to Mistland,<BR>and brought a creature from Mistland... Mistland's Goddess Vestinel.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-g" image="QN_NPC_Lunaria.dds" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-h" mid="105" select_type="2" />
      <talk_question mid="23"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Vestinel?!! Lunaria is Goddess Vestinel?!?!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-h" image="QN_NPC_Lunaria.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-i" mid="106" select_type="2" />
      <talk_question mid="24"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_03.ogg}<BR>But the device they created is imperfect, and only managed to bring is a small figment of Goddess Vestinel. They call this figment "Lunaria".</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-i" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-j" mid="107" select_type="2" />
      <talk_question mid="25"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_04.ogg}<BR>Goddess Vestinel's only goal is to destroy Verathea and drown it in darkness.<BR>Her fragment Lunaria must have the same goal as her, so the first thing she's aiming for must be the Monolith's door.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-j" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-k" mid="108" select_type="2" />
      <talk_question mid="26"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_05.ogg}<BR>The moment she opens the door, the world will start to crumble. The nightmares of the Goddess will send signals to Vestinel, who will come to completely destroy our world which would have turned into hell by then.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-k" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-l" mid="109" select_type="2" />
      <talk_question mid="27"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_06.ogg}<BR>To put it shortly, it will be the apocalypse.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-l" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-m" mid="110" select_type="2" />
      <talk_question mid="28"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>What...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-m" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-n" mid="111" select_type="2" />
      <talk_question mid="29"><![CDATA[<html>$Ancient 1$<BR>Can we do anything to stop Goddess Vestinel's fragment?<BR><FONT color=#7e12d2>(The Ancients are terrified by the news)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-n" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-o" mid="112" select_type="2" />
      <talk_question mid="30"><![CDATA[<html>$Gaharam$<BR>She is the poison that came here to destroy our world. Even if we can use some of Goddess's powers, we are still humans. We can't do anything to destroy her. The only person who can do that would be herself and... Goddess Vestinel.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-o" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-p" mid="113" select_type="2" />
      <talk_question mid="31"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>No way!!!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-p" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-q" mid="114" select_type="2" />
      <talk_question mid="32"><![CDATA[<html>$Ancient 2$<BR>We can't let the world be destroyed like this!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-q" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-r" mid="115" select_type="2" />
      <talk_question mid="33"><![CDATA[<html>$Ancient 3$<BR>She's just a fragment of the Goddess! Maybe we can fight her if we join all our forces!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-r" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-s" mid="116" select_type="2" />
      <talk_question mid="34"><![CDATA[<html>$Gaharam$<BR>The end is near for our world since someone who is bent on destroying it has appeared. But... maybe father would have a solution.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-s" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-t" mid="117" select_type="2" />
      <talk_question mid="35"><![CDATA[<html>$Ancient 4$<BR>How are we going to purify the poison of corruption?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-t" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-u" mid="118" select_type="2" />
      <talk_question mid="36"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_07.ogg}<BR>Soon my mother will complete the Mirror of Truth. That's our only hope.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-u" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-3-v" mid="120" select_type="2" />
      <talk_question mid="37"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_08.ogg}<BR>I will take it to my father. Since they have already summoned Lunaria, they will wait no further. Please Pergilah ke <FONT color=#a82424>Praying Heart</FONT>. The Monolith's door is there.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-3-v" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-4" mid="121" select_type="2" />
      <talk_question mid="38"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Monolith's door...! Would it be the same as the one I saw at the Abyss of Heat?)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-4" image="" itemindex="" portrait_left="NPC_Left_Ancient_minion.dds" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-4-" mid="122" select_type="5" />
      <talk_question mid="39"><![CDATA[<html>$Gaharam$<BR>Please go ahead first.<BR>I will follow with my human friend.<BR><BR><FONT color=#7e12d2>(Talk to Gaharam after the Ancients are gone)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-a" mid="123" select_type="2" />
      <talk_question mid="40"><![CDATA[<html>$Gaharam$<BR>First of all, I would like to thank you for your help.<BR>I didn't expect that place to be filled with poison now... They have sinned beyond redemption.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-a" image="QN_Npc_Arno.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-b" mid="124" select_type="2" />
      <talk_question mid="41"><![CDATA[<html>$Gaharam$<BR>My own father has done such terrible things...<BR>My mother would be so disheartened when she finds out tentang this. She's already weak from the injuries...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-b" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-c" mid="125" select_type="2" />
      <talk_question mid="42"><![CDATA[<html>${user_nick}$<BR><FONT color=#a82424>Gaharam...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-c" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-d" mid="126" select_type="2" />
      <talk_question mid="43"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_09.ogg}<BR>The Mirror of Truth will soon be completed. But... I'm afraid we would become corrupted when we get there. <FONT color=#387206>{user_nick}</FONT>, if that happens, please let the whole world know tentang our foolish mistakes.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-d" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-e" mid="127" select_type="2" />
      <talk_question mid="44"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>What?!?!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-e" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-f" mid="128" select_type="2" />
      <talk_question mid="45"><![CDATA[<html>$Gaharam$<BR>I'm sure you can do it. As a Prophet, I know that you can.<BR>The noble power of the dragon will protect you from corruption.<BR>Even if we become corrupted, you will remain the same.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-f" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-g" mid="129" select_type="2" />
      <talk_question mid="46"><![CDATA[<html>$Gaharam$<BR>Even we, the people who has the power of the Goddess, don't have the blessing of dragons. I envy you. You can't win someone's heart with just strength.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-g" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-h" mid="130" select_type="2" />
      <talk_question mid="47"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-h" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-i" mid="132" select_type="2" />
      <talk_question mid="48"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_10.ogg}<BR>We may have the power of Gods, but what good is it if that makes us hated by all the others? We're monsters. We need to repent for the rest of our lives.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-i" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-j" mid="133" select_type="2" />
      <talk_question mid="49"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Gaharam... She's more thoughtful than the other Ancients)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-j" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-k" mid="134" select_type="2" />
      <talk_question mid="50"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_11.ogg}<BR>Perhaps the Goddess and the world would forgive us at the end of our atonement. It's okay even if we are not forgiven. I just wish we wouldn't be ostracized from the world...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-k" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-l" mid="135" select_type="2" />
      <talk_question mid="51"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_12.ogg}<BR>You've appeared suddenly before us, with the power of noble dragon in you. May I take this as a sign that the Goddess has not forsaken us yet?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-l" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-m" mid="136" select_type="2" />
      <talk_question mid="52"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Erm... that's...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-m" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-n" mid="138" select_type="2" />
      <talk_question mid="53"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_13.ogg}<BR>I'm sorry. It may be too hard for you to understand what I feel.<BR>I wish I had more time with you.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-n" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-o" mid="139" select_type="2" />
      <talk_question mid="54"><![CDATA[<html>$Gaharam${sound:npc_Gaharam_9441_qu_14.ogg}<BR>Come to think of it, I still don't know your name. I apologize for asking this too late. Could you tell me your name?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-o" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-class_check_0" mid="140" select_type="2" />
      <talk_question mid="55"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I'm {user_nick}....</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-p" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-r" mid="141" select_type="2" />
      <talk_question mid="56"><![CDATA[<html>$Gaharam$<BR>I see. That's a nice name.<BR><FONT color=#7e12d2>(Gaharam smiles)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-q" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-r" mid="142" select_type="2" />
      <talk_question mid="57"><![CDATA[<html>$Gaharam$<BR>...! I see... That's a nice name...<FONT color=#7e12d2>(Gaharam smiles)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-r" image="QW_97.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-s" mid="144" select_type="2" />
      <talk_question mid="58"><![CDATA[<html>$Gaharam$<BR>Let's hurry to the <FONT color=#a82424>Praying Heart</FONT> now. Goddess, please watch over us...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-s" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-t" mid="145" select_type="2" />
      <talk_question mid="59"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(What should I do...?)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-t" image="QN_Npc_Arno.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-u" mid="146" select_type="2" />
      <talk_question mid="60"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(I already know what's going to happen. They'll suceed in turning Arno back to his original self)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-u" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-v" mid="147" select_type="2" />
      <talk_question mid="61"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(But the Monolith will open, and the world will be on the verge of destruction beyond redemption)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-v" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-w" mid="148" select_type="2" />
      <talk_question mid="62"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Arno will kill all the Ancients and create the seal stone so that Argenta doesn't have to sacrifice herself...)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-w" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-5-x" mid="150" select_type="2" />
      <talk_question mid="63"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(And Gaharam would be the only one left... but her fate is...)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-5-x" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6" mid="151" select_type="2" />
      <talk_question mid="64"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(The Ancients were awful people, but is death the right ending they deserved?)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-a" mid="152" select_type="2" />
      <talk_question mid="65"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(I wish I didn't know tentang this... what should I do? Should I keep helping them?)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-a" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-b" mid="153" select_type="2" />
      <talk_question mid="66"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Wait a minute... I'm not a vision or a wisp here. I'm really existing in the past! Maybe I can change what happens in the future. Then the Ancients won't make their mistake, and the world would be saved from the catastrophe.)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-b" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-class_check_2" mid="154" select_type="2" />
      <talk_question mid="67"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Yes... I should try even if the chance may be slim! I'll follow Gaharam and stop the corrupted Ancients. The Monolith will stay closed, and Argenta can be saved too.)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-c" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-d" mid="156" select_type="2" />
      <talk_question mid="68"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(But even if this is the real past, I couldn't stop anything with my knowledge of the future. It has always been that way. I'm sure I can't stop anything...)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-d" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-e" mid="157" select_type="2" />
      <talk_question mid="69"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(...but I can't give up...)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-e" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-f" mid="158" select_type="2" />
      <talk_question mid="70"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(...)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-f" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-g" mid="159" select_type="2" />
      <talk_question mid="71"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(No I can't! Not doing anything because I know a terrible future lies ahead is being a coward. I will do whatever I can to save lives!)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-g" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-h" mid="160" select_type="2" />
      <talk_question mid="72"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(I don't want to see people around me dying anymore)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-h" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-i" mid="161" select_type="2" />
      <talk_question mid="73"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Never...)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-i" image="QW_97.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1577_gaharam-6-j" mid="165" select_type="2" />
      <talk_question mid="74"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(I will stop them. To the </FONT><FONT color=#a82424>Praying Heart</FONT><FONT color=#662500> it is! I'll stop the tragedy!)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1577_gaharam-6-j" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1577_gaharam-6-k" mid="166" select_type="4" />
      <talk_question mid="162"><![CDATA[<html>$Quest Complete$</html>]]></talk_question>
   </talk_paragraph>
