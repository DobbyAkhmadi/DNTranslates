   <talk_paragraph index="n2978_hwaran-4-n" image="" itemindex="" portrait_left="Npc_Left_hwaran.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2978_hwaran-4-o" mid="88" select_type="2" />
