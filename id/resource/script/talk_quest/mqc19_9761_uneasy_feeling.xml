   <talk_paragraph index="n2062_hologram_nophee-2" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Hologram.dds" center_image="">
      <talk_answer link_index="n2062_hologram_nophee-2-" mid="769" select_type="5" />
      <talk_question mid="709"><![CDATA[<html>$Hologram Norfe$<BR>Pergilah kepada Sorceress Anna. Ia pasti sudah menyiapkan obat. </html>]]></talk_question>
