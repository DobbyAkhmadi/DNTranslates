   <talk_paragraph index="n2062_hologram_nophee-1-a" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Hologram.dds" center_image="">
      <talk_answer link_index="n2062_hologram_nophee-1-b" mid="764" select_type="2" />
      <talk_question mid="704"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Gerraint berkata Black Lake adalah tempat terbaik untuk menyembunyikan jejak Rubinart dalam kabut. </FONT></html>]]></talk_question>
