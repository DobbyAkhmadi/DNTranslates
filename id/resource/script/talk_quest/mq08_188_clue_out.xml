      <talk_answer link_index="n1101_lunaria-2-k" mid="381" select_type="0"><![CDATA[<html>Apa artinya?</html>]]></talk_answer>
      <talk_answer link_index="n1101_lunaria-2-k" mid="382" select_type="0"><![CDATA[<html>Kamu mengetahui sesuatu, iya kan?</html>]]></talk_answer>
