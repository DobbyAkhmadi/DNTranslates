   <talk_paragraph index="n037_geraint-2-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint.dds" center_image="">
      <talk_answer link_index="n037_geraint-2-c" mid="90" select_type="2" />
      <talk_question mid="89"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>A nest? What do you mean?</FONT></html>]]></talk_question>
