   <talk_paragraph index="n042_general_duglars-1-c" image="QN_Rose.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-" mid="130" select_type="5" />
