   <talk_paragraph index="n037_geraint-2-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint.dds" center_image="">
      <talk_answer link_index="n037_geraint-2-b" mid="88" select_type="2" />
      <talk_question mid="18"><![CDATA[<html>$Geraint$<BR>A <FONT color=#a82424>nest</FONT> has already been formed inside. I can't go any further.</html>]]></talk_question>
