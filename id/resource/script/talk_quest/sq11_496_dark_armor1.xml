   <talk_paragraph index="n091_trader_bellin-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n091_trader_bellin-accepting-b" mid="10" select_type="0"><![CDATA[<html>(Dia keras...)</html>]]></talk_answer>
      <talk_question mid="9"><![CDATA[<html>$Blacksmith Berlin$<BR>Kamu terlihat pemula. Aku tak punya waktu untukmu. Pergilah!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n091_trader_bellin-accepting-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n091_trader_bellin-accepting-c" mid="12" select_type="0"><![CDATA[<html>Apa?</html>]]></talk_answer>
      <talk_question mid="11"><![CDATA[<html>$Blacksmith Berlin$<BR>Hmm? Tunggu sebentar! ... Apa itu?</html>]]></talk_question>
