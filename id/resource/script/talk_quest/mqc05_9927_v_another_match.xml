   </talk_paragraph>
   <talk_paragraph index="n2872_kanna-2-f" image="" itemindex="" portrait_left="Npc_Left_Kanna.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2872_kanna-2-g" mid="128" select_type="2" />
