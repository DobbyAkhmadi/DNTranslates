   </talk_paragraph>
   <talk_paragraph index="n032_warrior_master_chandler-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n032_warrior_master_chandler-2-member1" mid="32" select_type="0"><![CDATA[<html>(Aku siap)</html>]]></talk_answer>
      <talk_answer link_index="n032_warrior_master_chandler-2-c" mid="33" select_type="0"><![CDATA[<html>(Aku akan kembali nanti)</html>]]></talk_answer>
