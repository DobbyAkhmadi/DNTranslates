   <talk_paragraph index="n711_warrior_fedro-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n711_warrior_fedro-2-a" mid="58" select_type="0"><![CDATA[<html>(Jelaskan situasinya)</html>]]></talk_answer>
