   <talk_paragraph index="n711_warrior_fedro-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n711_warrior_fedro-accepting-a" mid="7" select_type="0"><![CDATA[<html>Apa begitu?</html>]]></talk_answer>
      <talk_question mid="6"><![CDATA[<html>$Master Warrior Pedro$<BR>Oh, kamu di sini.<BR>Reputasimu cukup bagus di Lotus Marsh ini!!</html>]]></talk_question>
