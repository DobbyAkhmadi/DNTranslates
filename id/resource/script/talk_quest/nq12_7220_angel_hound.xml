   <talk_paragraph index="n193_santaund-accepting" image="QN_Npc_Saint_BlackSmith_Dwarf.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n193_santaund-accepting-a" mid="10" select_type="0"><![CDATA[<html>(SantaHound sepertinya sedang tidak enak hati.)</html>]]></talk_answer>
      <talk_question mid="9"><![CDATA[<html>$SantaHound$<BR><FONT color=#7e12d2(Beritahu tentang permintaan Blacksmith Berlin)</FONT><BR>Apakah Blacksmith itu membutuhkan bantuanku?<BR>Kuruung, benar-benar manusia yang tidak tahu malu.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n193_santaund-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n193_santaund-accepting-acceptted" mid="12" select_type="0"><![CDATA[<html><FONT color=#086171>Terima Quest</FONT> </html>]]></talk_answer>
