   <talk_paragraph index="n2688_guest2-3" image="" itemindex="" portrait_left="NPC_Left_Sorceress_Tessa.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2688_guest2-3-a" mid="89" select_type="2" />
