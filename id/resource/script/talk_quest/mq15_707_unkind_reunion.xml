   <talk_paragraph index="n707_sidel-accepting-a" image="QN_Npc_OldKarakule.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-accepting-acceptted" mid="86" select_type="3" />
