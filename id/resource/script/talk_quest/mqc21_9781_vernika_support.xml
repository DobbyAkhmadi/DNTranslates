   <talk_paragraph index="n2266_grele-4-f" image="" itemindex="" portrait_left="Npc_Left_child_grele.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2266_grele-4-g" mid="114" select_type="2" />
      <talk_question mid="113"><![CDATA[<html>$Grelle$<BR>Karena itu kakak juga pasti bisa dikembalikan seperti semula!</html>]]></talk_question>
