   <talk_paragraph index="n2266_grele-4-c" image="" itemindex="" portrait_left="Npc_Left_child_grele.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2266_grele-4-d" mid="108" select_type="2" />
      <talk_question mid="107"><![CDATA[<html>$Grelle$<BR>A, aku baik-baik saja! Aku ingin membantu kalian mencari Healing Cup!</html>]]></talk_question>
