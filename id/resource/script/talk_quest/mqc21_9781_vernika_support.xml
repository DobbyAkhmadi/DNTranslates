      <talk_answer link_index="n2266_grele-4-a" mid="102" select_type="2" />
      <talk_question mid="101"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Grelle? Bagaimana kau bisa ikut?</FONT></html>]]></talk_question>
