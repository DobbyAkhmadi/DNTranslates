      <talk_answer link_index="n2266_grele-4-e" mid="110" select_type="2" />
      <talk_question mid="109"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Tapi Grelle…</FONT></html>]]></talk_question>
