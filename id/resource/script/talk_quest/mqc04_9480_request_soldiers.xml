   <talk_paragraph index="n039_bishop_ignasio-accepting" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Bishop.dds" center_image="">
      <talk_answer link_index="n039_bishop_ignasio-accepting-acceptted" mid="6" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Bishop Ignacio$<BR>Kita harus mengirim elite soldiers. </html>]]></talk_question>
