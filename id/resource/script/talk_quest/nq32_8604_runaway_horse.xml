   <talk_paragraph index="n062_trainer_lindsay-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n062_trainer_lindsay-accepting-b" mid="141" select_type="0"><![CDATA[<html>Kemana dia pergi?</html>]]></talk_answer>
