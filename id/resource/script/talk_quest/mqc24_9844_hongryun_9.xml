   <talk_paragraph index="n2655_hongryun_ju-1-a" image="" itemindex="" portrait_left="Npc_Left_child_grele.dds" portrait_right="Npc_Right_hongryun_guard.dds.dds" center_image="">
      <talk_answer link_index="n2655_hongryun_ju-1-b" mid="57" select_type="2" />
      <talk_question mid="54"><![CDATA[<html>$Guard$<BR>We would like to speak to the head merchant.</html>]]></talk_question>
