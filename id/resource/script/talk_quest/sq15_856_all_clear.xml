   <talk_paragraph index="n713_soceress_tamara-2-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n713_soceress_tamara-2-d" mid="147" select_type="0"><![CDATA[<html>(Jelaskan situasinya)</html>]]></talk_answer>
      <talk_question mid="146"><![CDATA[<html>$Master Sorceress Tamara$<BR>Ah!! Kenapa kamu di sini?<BR><FONT color=#7e12d2>(Tamara bertanya)</FONT></html>]]></talk_question>
