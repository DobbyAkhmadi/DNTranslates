   <talk_paragraph index="n004_guard_steave-1-a" image="QS_Default.dds" itemindex="300009" portrait_left="" portrait_right="NPC_Right_Guard_Steave.dds" center_image="">
      <talk_answer link_index="n004_guard_steave-1-b" mid="103" select_type="2" />
