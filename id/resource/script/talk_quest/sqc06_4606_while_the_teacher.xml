   <talk_paragraph index="n1704_teramai_west-2-j" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1704_teramai_west-2-!next" mid="39" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="38"><![CDATA[<html>$Terrence Terramai$<BR>Jangan terlalu kecewa walaupun jika itu tidak membantu.</html>]]></talk_question>
   </talk_paragraph>
