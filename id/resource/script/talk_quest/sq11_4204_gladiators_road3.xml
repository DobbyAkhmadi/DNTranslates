   <talk_paragraph index="n249_saint_guard-1-f" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n249_saint_guard-1-g" mid="57" select_type="0"><![CDATA[<html>Oh begitu.</html>]]></talk_answer>
