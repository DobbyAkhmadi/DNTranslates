   <talk_paragraph index="n012_sorceress_master_cynthia-3-b" image="QS_Default.dds" itemindex="300059" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n012_sorceress_master_cynthia-3-c" mid="17" select_type="0"><![CDATA[<html>(Menceritakan pada Cynthia tentang orang bertopeng dengan jubah merah.)</html>]]></talk_answer>
      <talk_question mid="16"><![CDATA[<html>$Master Sorceress Cynthia$<BR>Bagaimanapun juga...terima kasih. <FONT color=#a82424>Vision Orb</FONT> adalah harta yang paling berharga bagi para Sorceress.</html>]]></talk_question>
