   <talk_paragraph index="n014_cleric_cedric-5-a" image="QS_Default.dds" itemindex="300059" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n014_cleric_cedric-5-b" mid="44" select_type="0"><![CDATA[<html>Jangan khawatir. Cynthia sudah menyimpannya....</html>]]></talk_answer>
      <talk_question mid="43"><![CDATA[<html>$Cleric Cedric$<BR>Benarkah sudah aman?<BR>Dimana lokasinya?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n014_cleric_cedric-5-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n014_cleric_cedric-5-c" mid="46" select_type="0"><![CDATA[<html>Aku harus pergi sekarang.</html>]]></talk_answer>
