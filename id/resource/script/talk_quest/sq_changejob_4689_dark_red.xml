   <talk_paragraph index="n458_sword_of_geraint-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n458_sword_of_geraint-1-a" mid="11" select_type="0"><![CDATA[<html>(Sosok Gerraint terlihat)</html>]]></talk_answer>
