   <talk_paragraph index="n2067_nophee-1-a" image="QN_hongryun_pether.dds" itemindex="" portrait_left="Npc_Left_Nophee.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2067_nophee-1-b" mid="105" select_type="2" />
      <talk_question mid="8"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Glad to hear it. Oh... Did you introduce each other?</FONT></html>]]></talk_question>
