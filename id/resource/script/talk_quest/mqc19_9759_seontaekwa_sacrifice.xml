   <talk_paragraph index="n2091_refugee_girl-4-a" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_girl.dds" center_image="">
      <talk_answer link_index="n2091_refugee_girl-4-b" mid="669" select_type="2" />
      <talk_question mid="603"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Tak mungkin.. Apakah mengalami serangan dari calamity Army? Waktu aku datang dari belakang tidak ada apapun... </FONT></html>]]></talk_question>
