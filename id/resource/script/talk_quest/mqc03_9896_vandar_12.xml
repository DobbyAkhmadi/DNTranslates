   <talk_paragraph index="n2808_hwaran-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2808_hwaran-accepting-acceptted" mid="593" select_type="3" />
      <talk_question mid="563"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>…… No, I don't think so. The group seems to have returned safely.</FONT></html>]]></talk_question>
