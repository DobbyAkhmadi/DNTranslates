   <talk_paragraph index="n277_bluff_dealers_pope-4-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n277_bluff_dealers_pope-4-b" mid="97" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="96"><![CDATA[<html>$Merchant Pero$<BR>Dia cukup pintar. Aku tak pernah menyangka dia mendapatkan Crown Apple tanpa harus membayar sepeserpun... Hmm... dia cukup pintar.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n277_bluff_dealers_pope-4-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n277_bluff_dealers_pope-4-c" mid="99" select_type="0"><![CDATA[<html>(<FONT color=#086171>Terima Reward)</FONT></html>]]></talk_answer>
