   <talk_paragraph index="n2294_grugiru-1-c" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_grugiru.dds" center_image="">
      <talk_answer link_index="n2294_grugiru-1-d" mid="16" select_type="2" />
