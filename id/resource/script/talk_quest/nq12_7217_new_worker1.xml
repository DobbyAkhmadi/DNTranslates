   <talk_paragraph index="n277_bluff_dealers_pope-1-g" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n277_bluff_dealers_pope-2" mid="46" select_type="0"><![CDATA[<html>Pekerjaan seperti apa?</html>]]></talk_answer>
