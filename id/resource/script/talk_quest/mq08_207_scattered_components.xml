   <talk_paragraph index="n099_engineer_hubert-3" image="QP_AirShip_part .dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Engineer_Hubert.dds" center_image="">
      <talk_answer link_index="n099_engineer_hubert-3-" mid="107" select_type="5" />
