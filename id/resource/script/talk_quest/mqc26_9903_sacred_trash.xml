   <talk_paragraph index="n2725_norphee-6-d" image="QN_Salica.dds" itemindex="" portrait_left="Npc_Left_Nophee.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2725_norphee-6-e" mid="133" select_type="2" />
      <talk_question mid="37"><![CDATA[<html>$Norfe$<BR>He knows what happened to <FONT color=#a82424>Shalica</FONT>, and in great detail.</html>]]></talk_question>
