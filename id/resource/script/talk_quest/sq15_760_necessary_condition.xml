   <talk_paragraph index="n703_book_doctor-2-h" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n703_book_doctor-2-i" mid="38" select_type="0"><![CDATA[<html>(Merobek beberapa sudut buku)</html>]]></talk_answer>
      <talk_question mid="37"><![CDATA[<html>$Chatterbox Book Doctor$<BR>Per, pertama... pergi ke library! Lalu baca buku mengenai liberal arts... lalu kembangkan pengetahuanmu...Ahhhhh!!!</html>]]></talk_question>
