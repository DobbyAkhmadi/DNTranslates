   <talk_paragraph index="n725_charty-4-b" image="QN_Npc_Sidel.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Charty.dds" center_image="">
      <talk_answer link_index="n725_charty-4-c" mid="134" select_type="2" />
      <talk_question mid="64"><![CDATA[<html>$Charti$<BR>I can get the medicine to my tribesmen. I want you to check on <FONT color=#a82424>Sidel.</FONT> He is a very sensitive boy...</html>]]></talk_question>
