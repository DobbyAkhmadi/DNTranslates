   <talk_paragraph index="n724_karahan-2-b" image="QW_51.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_karahan.dds" center_image="">
      <talk_answer link_index="n724_karahan-2-" mid="129" select_type="5" />
      <talk_question mid="107"><![CDATA[<html>$Karahan$<BR><FONT color=#a82424> Contaminated Marsh Moss</FONT> grows in the <FONT color=#a82424>Ancient Library</FONT> and <FONT color=#a82424>Jungle Bracken</FONT> can be found at the <FONT color=#a82424>Riverwort Ruins.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n724_karahan-3" image="QS_Default.dds" itemindex="300385" portrait_left="" portrait_right="NPC_Right_karahan.dds" center_image="">
