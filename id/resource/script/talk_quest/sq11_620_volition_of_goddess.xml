   <talk_paragraph index="n277_bluff_dealers_pope-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n277_bluff_dealers_pope-1-a" mid="88" select_type="0"><![CDATA[<html>Ini tidaklah menyenangkan ataupun hebat.</html>]]></talk_answer>
      <talk_answer link_index="n277_bluff_dealers_pope-1-a" mid="102" select_type="0"><![CDATA[<html>Berapa harganya?</html>]]></talk_answer>
      <talk_question mid="27"><![CDATA[<html>$Dishonest Merchant Pero$<BR>Hadiah...? Hadiah apa...?<BR>
