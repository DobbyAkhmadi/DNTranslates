   <talk_paragraph index="n775_cleric_ilizer-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n775_cleric_ilizer-accepting-a" mid="10" select_type="0"><![CDATA[<html>Pesan?</html>]]></talk_answer>
      <talk_question mid="9"><![CDATA[<html>$Temple Knight Ilizer$<BR><FONT color=#387206>{user_nick}</FONT>, apa kabar? Aku mendapatkan permintaan dari Priestess of Darkness Madeline untuk menyampaikan pesan ke kamu.</html>]]></talk_question>
