   <talk_paragraph index="n1067_elder_elf-1-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1067_elder_elf-1-b" mid="36" select_type="0"><![CDATA[<html>Darkness?</html>]]></talk_answer>
      <talk_question mid="35"><![CDATA[<html>$Elf Elder$<BR>
