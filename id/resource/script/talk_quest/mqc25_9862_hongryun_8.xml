   <talk_paragraph index="n2808_hwaran-2-b" image="QN_Jade.dds" itemindex="" portrait_left="Npc_Left_hwaran.dds" portrait_right="Npc_Right_Nophee.dds" center_image="">
      <talk_answer link_index="n2808_hwaran-2-c" mid="69" select_type="2" />
      <talk_question mid="18"><![CDATA[<html>$Norfe$<BR>In fact, we did a background check on this <FONT color=#a82424>wolf</FONT>.<BR>You said taking this job might put our life on the line, after all.</html>]]></talk_question>
