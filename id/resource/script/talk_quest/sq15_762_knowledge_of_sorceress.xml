   <talk_paragraph index="n473_great_book-1-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n473_great_book-1-g" mid="97" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="96"><![CDATA[<html>$The Greatest Book Solomon$<BR><FONT color=#a82425>Pyromancer</FONT> memfokuskan skill serangan api dari  Elemental Lord. Ahli dalam skill serangan api jarak jauh dan juga area yang lebar. Kamu akan menjadi Sorceress yang memiliki area serangan yang besar.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n473_great_book-1-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n473_great_book-1-g" mid="99" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="98"><![CDATA[<html>$The Greatest Book Solomon$<BR><FONT color=#a82425>Cryomancer</FONT>menggunakan skill serangan dingin Elemental Lord . Sangat hebat dengan serangan es yang dapat memperlambat lawan dari jarak menengah namun lebar. Kamu akan menjadi sub-Sorceress yang memiliki area serangan menengah-lebar.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n473_great_book-1-d" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n473_great_book-1-f" mid="101" select_type="0"><![CDATA[<html>Tanyakan mengenai pengetahuan Smasher◁ </html>]]></talk_answer>
      <talk_answer link_index="n473_great_book-1-e" mid="102" select_type="0"><![CDATA[<html>Tanyakan mengenai pengetahuan Majesty◁ </html>]]></talk_answer>
