   </talk_paragraph>
   <talk_paragraph index="n1568_lunaria-2-x" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1568_lunaria-2-y" mid="156" select_type="2" />
