   <talk_paragraph index="n1504_hound_ghost-3-b" image="QN_Npc_lifius.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1504_hound_ghost-3-!next" mid="36" select_type="0"><![CDATA[<html>(Lanjutkan)</html>]]></talk_answer>
      <talk_question mid="35"><![CDATA[<html>$Ghost Hound$<BR>Kalau aku pikir-pikir...<BR>Memberitahu Loffy dimana Lipius tidak akan cukup...</html>]]></talk_question>
