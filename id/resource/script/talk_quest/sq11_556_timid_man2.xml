   <talk_paragraph index="n133_lena_duglars-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n133_lena_duglars-accepting-b" mid="10" select_type="0"><![CDATA[<html>(Lena memeriksa sakunya)</html>]]></talk_answer>
      <talk_question mid="9"><![CDATA[<html>$Miss Lena$<BR>...Astaga, jadi kalung itu...<BR>Ya, Sekarang aku ingat sesuatu.</html>]]></talk_question>
