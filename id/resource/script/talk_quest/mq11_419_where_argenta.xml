   <talk_paragraph index="n039_bishop_ignasio-accepting-c" image="QN_Npc_Argenta.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Bishop.dds" center_image="">
      <talk_answer link_index="n039_bishop_ignasio-accepting-acceptted" mid="102" select_type="3" />
