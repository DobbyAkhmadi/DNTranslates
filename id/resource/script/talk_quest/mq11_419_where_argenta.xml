      <talk_answer link_index="n039_bishop_ignasio-accepting-b" mid="99" select_type="2" />
      <talk_question mid="98"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Ya. Dia selalu mengejar Black Knight.</FONT></html>]]></talk_question>
