   <talk_paragraph index="n039_bishop_ignasio-accepting" image="QN_Npc_Velskud1.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Bishop.dds" center_image="">
      <talk_answer link_index="n039_bishop_ignasio-accepting-a" mid="97" select_type="2" />
      <talk_question mid="92"><![CDATA[<html>$Bishop Ignacio$<BR>Is there really somebody who knows the whereabouts of the <FONT color=#a82424>Black Knight</FONT>?<BR>Is this true?</html>]]></talk_question>
