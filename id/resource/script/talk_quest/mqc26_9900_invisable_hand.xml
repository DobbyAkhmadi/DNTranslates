   <talk_paragraph index="n2978_hwaran-1-d" image="" itemindex="" portrait_left="Npc_Left_hwaran.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2978_hwaran-1-e" mid="87" select_type="2" />
      <talk_question mid="12"><![CDATA[<html>$Commander Hwaran${sound:npc_Hwaran_qu9900_sp_01.ogg}<BR>How very interesting. You brought back the most useless and the most precious items from the <FONT color=#086171>Silver Treasure Vault</FONT>.</html>]]></talk_question>
