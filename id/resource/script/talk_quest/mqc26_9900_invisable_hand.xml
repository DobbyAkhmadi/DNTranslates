      <talk_answer link_index="n2983_vandar-2-f" mid="110" select_type="2" />
      <talk_question mid="31"><![CDATA[<html>$Kali$<BR><FONT color=#662500>……??? You're... a mercenary?</FONT></html>]]></talk_question>
