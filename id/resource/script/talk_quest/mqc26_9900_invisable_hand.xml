   <talk_paragraph index="n2978_hwaran-1-m" image="" itemindex="" portrait_left="Npc_Left_hwaran.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2978_hwaran-1-n" mid="96" select_type="2" />
      <talk_question mid="21"><![CDATA[<html>$Commander Hwaran$<BR>Yes, the <FONT color=#086171>Plentiful Gold Trove</FONT> is the true treasure vault filled with riches from around the world.<BR>If you go through the left door, you'll see the <FONT color=#086171>garden</FONT>. The answer is at <FONT color=#a82424>the end of the road</FONT> through the pond.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2978_hwaran-1-n" image="QS_Default.dds" itemindex="400725" portrait_left="Npc_Left_hwaran.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2978_hwaran-1-o" mid="97" select_type="2" />
      <talk_question mid="22"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>So I go there and open it with this stone, right?</FONT></html>]]></talk_question>
