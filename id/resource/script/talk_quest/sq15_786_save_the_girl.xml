   <talk_paragraph index="n722_cat_mewmew-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n722_cat_mewmew-accepting-a" mid="6" select_type="0"><![CDATA[<html>Apa?! Kemana dia pergi?</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$All-round Cat$<BR>Ya tuhan!! Aku dalam masalah besar. Majikanku...belum juga kembali!</html>]]></talk_question>
