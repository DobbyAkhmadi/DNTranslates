   <talk_paragraph index="n2518_magnate_gilbert-accepting" image="" itemindex="" portrait_left="NPC_Right_alrento.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2518_magnate_gilbert-accepting-acceptted" mid="6" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Merchant Gilbert$<BR>Well, there are aspects of it that suggest it could be real.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2518_magnate_gilbert-1" image="" itemindex="" portrait_left="NPC_Right_alrento.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2518_magnate_gilbert-1-" mid="8" select_type="5" />
