   </talk_paragraph>
   <talk_paragraph index="n2815_salica-2-i" image="" itemindex="" portrait_left="Npc_Left_Salica.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2815_salica-2-j" mid="93" select_type="2" />
