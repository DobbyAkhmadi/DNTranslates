   <talk_paragraph index="n2811_mugeuk-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2811_mugeuk-1-a" mid="63" select_type="2" />
      <talk_question mid="10"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I saw this in the Blue Raven Territory.<BR>This is the symbol on the feathers of the Blue Clan, right?</FONT><BR><FONT color=#662500>Though this one looks a lot less sophisticated.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2811_mugeuk-1-a" image="QS_Default.dds" itemindex="400673" portrait_left="Npc_Left_hongryun_pether_mask.dds.dds" portrait_right="" center_image="">
