      <talk_answer link_index="n2815_salica-2-l" mid="95" select_type="3" />
      <talk_question mid="31"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Here, everybody take one each.<BR><BR></FONT><FONT color=#7e12d2>(Hands a jar with a Quality Certificate attached to each party member.)</FONT></html>]]></talk_question>
