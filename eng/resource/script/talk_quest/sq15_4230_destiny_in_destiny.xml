   <talk_paragraph index="n1023_academic-1-t" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1023_academic-2" mid="75" select_type="0"><![CDATA[<html>…Okay.</html>]]></talk_answer>
      <talk_answer link_index="n1023_academic-2" mid="108" select_type="0"><![CDATA[<html>…I understand. Don't push yourself too hard.</html>]]></talk_answer>
