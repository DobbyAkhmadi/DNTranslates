   <talk_paragraph index="n2062_hologram_nophee-accepting" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Hologram.dds" center_image="">
      <talk_answer link_index="n2062_hologram_nophee-accepting-acceptted" mid="372" select_type="3" />
      <talk_question mid="341"><![CDATA[<html>$Hologram Norfe$<BR>No, it's not your fault.You didn't make a wrong decision. </html>]]></talk_question>
