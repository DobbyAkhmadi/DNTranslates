   <talk_paragraph index="n042_general_duglars-3-e" image="" itemindex="" portrait_left="NPC_Left_Kalaen.dds" portrait_right="" center_image="">
      <talk_answer link_index="n042_general_duglars-3-f" mid="60" select_type="2" />
      <talk_question mid="59"><![CDATA[<html>$Court Sorceress Kayleen$<BR>The only thing you knew was that he's a traitor? You useless child.<BR>Then we should start with Nameless Tyrant Tomb where the last report came in from.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-3-f" image="" itemindex="" portrait_left="NPC_Left_Kalaen.dds" portrait_right="" center_image="">
      <talk_answer link_index="n042_general_duglars-4" mid="62" select_type="2" />
