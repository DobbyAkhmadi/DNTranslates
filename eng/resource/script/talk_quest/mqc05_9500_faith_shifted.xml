   <talk_paragraph index="n1762_kaye-4-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Kaye.dds" center_image="">
      <talk_answer link_index="n1762_kaye-4-f" mid="143" select_type="2" />
      <talk_question mid="142"><![CDATA[<html>$Kay${sound:npc_Kaye_9500_qu_04.ogg}<BR>How should I report this to him. Ahhhh. This is all because of my greeeat friend...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1762_kaye-4-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Kaye.dds" center_image="">
      <talk_answer link_index="n1762_kaye-5" mid="88" select_type="2" />
      <talk_question mid="87"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I'm sorry.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1762_kaye-5" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Kaye.dds" center_image="">
      <talk_answer link_index="n1762_kaye-5-a" mid="90" select_type="2" />
      <talk_question mid="89"><![CDATA[<html>$Kay${sound:npc_Kaye_9500_qu_05.ogg}<BR>Pay this debt later if you really are. <BR>I'm going to do what I can to help you clean up this mess.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1762_kaye-5-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Kaye.dds" center_image="">
      <talk_answer link_index="n1762_kaye-5-" mid="92" select_type="5" />
      <talk_question mid="91"><![CDATA[<html>$Kay$<BR>Argh...<FONT color=#387206>{user_nick}</FONT>!! You should just go back home!</html>]]></talk_question>
