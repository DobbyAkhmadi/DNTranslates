   <talk_paragraph index="n005_archer_master_diana-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n005_archer_master_diana-2-a" mid="175" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="174"><![CDATA[<html>$Master Archer Diana$<BR>We meet again. It's been just a few moments since you left, but you look so different than before.<BR>It's like we also get old when we're here in humans' world.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n005_archer_master_diana-2-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n005_archer_master_diana-2-check1" mid="177" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
