      <talk_answer link_index="n1077_elf_guard_basha-2-b" mid="29" select_type="2" />
      <talk_question mid="28"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Yes. Are you alright?</FONT></html>]]></talk_question>
