      <talk_answer link_index="n726_velskud-4-j" mid="280" select_type="2" />
      <talk_question mid="279"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>So you ARE here for </FONT><FONT color=#a82424>Karahan.</FONT></html>]]></talk_question>
