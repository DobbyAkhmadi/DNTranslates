   <talk_paragraph index="n1145_lunaria-1-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1145_lunaria-1-" mid="291" select_type="5" />
