   <talk_paragraph index="n1880_guard_seef-3-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1880_guard_seef-3-b" mid="35" select_type="0"><![CDATA[<html>What is it about?</html>]]></talk_answer>
