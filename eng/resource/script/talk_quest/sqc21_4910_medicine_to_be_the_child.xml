   <talk_paragraph index="n1880_guard_seef-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1880_guard_seef-accepting-a" mid="6" select_type="0"><![CDATA[<html>So does the potion really exist?</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Guard Chiffe$<BR>Oh it's you, <FONT color=#387206>{user_nick}</FONT>.</html>]]></talk_question>
