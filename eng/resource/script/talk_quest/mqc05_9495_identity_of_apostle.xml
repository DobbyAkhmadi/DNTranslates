   <talk_paragraph index="n042_general_duglars-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-a" mid="12" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>The Dragon Followers took something precious to me…<BR>I have to retrieve it.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-1-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-b" mid="14" select_type="2" />
      <talk_question mid="13"><![CDATA[<html>$General Douglas$<BR>Oh no... I can feel your pain.<BR>How about talking to the other adventurers? They're after the Dragon Followers too.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-1-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-c" mid="16" select_type="2" />
      <talk_question mid="15"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I see! Where are they?<BR>(Can I meet Edan again?!!)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-1-c" image="QN_Npc_Bishop.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-d" mid="18" select_type="2" />
      <talk_question mid="17"><![CDATA[<html>$General Douglas$<BR>Well, I think they're helping <FONT color=#a82424>Bishop Ignacio</FONT> these days.<BR>Why don't you ask the Bishop?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-1-d" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-" mid="20" select_type="5" />
      <talk_question mid="19"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Thank you, General! Thank you!!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-2" image="QN_Npc_Bishop.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-2-" mid="22" select_type="5" />
      <talk_question mid="21"><![CDATA[<html>$General Douglas$<BR>The adventurers seem to be helping <FONT color=#a82424>Bishop Ignacio</FONT> these days.<BR>Why don't you ask the Bishop?</html>]]></talk_question>
