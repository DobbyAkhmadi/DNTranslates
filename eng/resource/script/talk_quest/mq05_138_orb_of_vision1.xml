   <talk_paragraph index="n020_sorceress_yullia-7" image="QN_SoceressCynthia.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sorceress_Tessa.dds" center_image="">
      <talk_answer link_index="n020_sorceress_yullia-7-a" mid="374" select_type="2" />
      <talk_question mid="269"><![CDATA[<html>$Sorceress Julia$<BR>>But the monsters are getting stronger, and all of us have retreated due to injuries. I'm worried about the Vision Orb.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n020_sorceress_yullia-7-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sorceress_Tessa.dds" center_image="">
      <talk_answer link_index="n020_sorceress_yullia-7-b" mid="375" select_type="2" />
      <talk_question mid="305"><![CDATA[<html>$Sorceress Julia$<BR>Only the sorceresses knew that the Vision Orb was being kept at Parelina's Resting Place. I don't know how the monsters found out about it. Anyway, we must protect the Orb.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n020_sorceress_yullia-7-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sorceress_Tessa.dds" center_image="">
      <talk_answer link_index="n020_sorceress_yullia-7-c" mid="376" select_type="4" />
      <talk_question mid="309"><![CDATA[<html>$Quest Reward$</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n020_sorceress_yullia-7-c" image="QW_8.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sorceress_Tessa.dds" center_image="">
      <talk_answer link_index="n020_sorceress_yullia-7-!next" mid="377" select_type="2" />
