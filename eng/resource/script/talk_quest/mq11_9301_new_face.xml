   </talk_paragraph>
   <talk_paragraph index="n1139_lunaria-2-a" image="QN_NPC_Illusion.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1139_lunaria-2-b" mid="136" select_type="2" />
