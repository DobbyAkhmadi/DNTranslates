   <talk_paragraph index="n1143_cian-3" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n1143_cian-3-a" mid="116" select_type="2" />
      <talk_question mid="115"><![CDATA[<html>$Warrior Xian$<BR>Ah, <FONT color=#387206>{user_nick}</FONT>, you're here too.<BR>We were just about to go back.</html>]]></talk_question>
