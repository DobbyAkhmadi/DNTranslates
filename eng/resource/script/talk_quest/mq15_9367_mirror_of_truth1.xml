   <talk_paragraph index="n822_teramai-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Teramai.dds" center_image="">
      <talk_answer link_index="n822_teramai-1-a" mid="20" select_type="2" />
      <talk_question mid="19"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Purity Mirror! Why didn't I think of that?<BR>Chatterbox Book Doctor some information about that recorded on him.<BR>Maybe it can be used again.)</FONT></html>]]></talk_question>
