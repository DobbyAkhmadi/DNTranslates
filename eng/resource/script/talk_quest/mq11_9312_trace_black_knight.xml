   <talk_paragraph index="n042_general_duglars-2-a" image="QN_NPC_Lunaria.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-2-" mid="1190" select_type="5" />
