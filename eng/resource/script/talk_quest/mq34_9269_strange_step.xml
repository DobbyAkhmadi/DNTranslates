   <talk_paragraph index="n1057_yuvenciel-3-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Yubaensiael.dds" center_image="">
      <talk_answer link_index="n1057_yuvenciel-3-c" mid="146" select_type="0"><![CDATA[<html>An apostle?</html>]]></talk_answer>
      <talk_answer link_index="n1057_yuvenciel-3-c" mid="147" select_type="0"><![CDATA[<html>Our enemy?</html>]]></talk_answer>
      <talk_answer link_index="n1057_yuvenciel-3-c" mid="148" select_type="0"><![CDATA[<html>What are you up to?</html>]]></talk_answer>
