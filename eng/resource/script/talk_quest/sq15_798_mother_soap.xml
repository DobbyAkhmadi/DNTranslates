   <talk_paragraph index="n714_cleric_yohan-1-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n714_cleric_yohan-2" mid="131" select_type="0"><![CDATA[<html>What ingredients?</html>]]></talk_answer>
