   <talk_paragraph index="n027_argenta-1-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n027_argenta-1-b" mid="290" select_type="2" />
      <talk_question mid="289"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I saw you in the data! But it's my first time seeing you for real!</FONT></html>]]></talk_question>
