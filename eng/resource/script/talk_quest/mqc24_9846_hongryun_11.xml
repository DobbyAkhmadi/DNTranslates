   <talk_paragraph index="n2714_conscience_illang-1-a" image="" itemindex="" portrait_left="Npc_Left_hongryun_senate.dds.dds" portrait_right="Npc_Right_hongryun_merchant.dds.dds" center_image="">
      <talk_answer link_index="n2714_conscience_illang-1-b" mid="57" select_type="2" />
      <talk_question mid="12"><![CDATA[<html>$Head Merchant Jamihwa${sound:npc_Hongryun_Jamihwa_qu_9842_sp_01.ogg}<BR>Yes, that’s right.<BR>But aren’t you the ones who added fuel to the rebellion?</html>]]></talk_question>
