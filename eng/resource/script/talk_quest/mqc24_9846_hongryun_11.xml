   <talk_paragraph index="n2714_conscience_illang-1-e" image="" itemindex="" portrait_left="Npc_Left_hongryun_senate.dds.dds" portrait_right="Npc_Right_hongryun_merchant.dds.dds" center_image="">
      <talk_answer link_index="n2714_conscience_illang-1-f" mid="61" select_type="2" />
      <talk_question mid="16"><![CDATA[<html>$Head Merchant Jamihwa${sound:npc_Hongryun_Jamihwa_qu_9842_sp_02.ogg}<BR>Then you should have killed him.<BR>Go back. I’ll ask about your errors once the rebellion is quelled.<BR>I’ll question every single one of you.</html>]]></talk_question>
   </talk_paragraph>
