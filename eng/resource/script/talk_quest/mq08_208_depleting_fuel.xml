   <talk_paragraph index="n099_engineer_hubert-4-d" image="QS_Default.dds" itemindex="300103" portrait_left="" portrait_right="NPC_Right_Engineer_Hubert.dds" center_image="">
      <talk_answer link_index="n099_engineer_hubert-4-" mid="122" select_type="5" />
      <talk_question mid="70"><![CDATA[<html>$Airship Engineer Hubert$<BR>Okay, go ahead. I'm desperate to make this thing fly again. I can't imagine how you're going to do it!</html>]]></talk_question>
