   <talk_paragraph index="n2062_hologram_nophee-accepting" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Hologram.dds" center_image="">
      <talk_answer link_index="n2062_hologram_nophee-accepting-acceptted" mid="71" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Hologram Norfe$<BR>That's a relief. He would probably come to his senses when he leaves the Black Lake. Can you tell me more about Rubinart's condition? </html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2128_geraint-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_geraint_Kid.dds" center_image="">
      <talk_answer link_index="n2128_geraint-1-" mid="99" select_type="5" />
      <talk_question mid="6"><![CDATA[<html>$Geraint$<BR>……………….</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2104_nophee-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Nophee.dds" center_image="">
      <talk_answer link_index="n2104_nophee-1-" mid="100" select_type="5" />
      <talk_question mid="7"><![CDATA[<html>$Unknown Boy$<BR>I'm collecting ingredients now.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2067_nophee-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Nophee.dds" center_image="">
      <talk_answer link_index="n2067_nophee-1-" mid="103" select_type="5" />
