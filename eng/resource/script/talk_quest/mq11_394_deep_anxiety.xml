   <talk_paragraph index="n216_geraint_wounded-accepting" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n216_geraint_wounded-accepting-a" mid="306" select_type="2" />
      <talk_question mid="8"><![CDATA[<html>$Geraint$<BR>I vowed to protect <FONT color=#a82424>Saint Haven</FONT>.<BR>
