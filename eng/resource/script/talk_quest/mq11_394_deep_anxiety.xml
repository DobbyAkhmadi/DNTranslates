   </talk_paragraph>
   <talk_paragraph index="n216_geraint_wounded-accepting-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n216_geraint_wounded-accepting-acceptted" mid="308" select_type="3" />
      <talk_question mid="307"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Please let me help.</FONT></html>]]></talk_question>
