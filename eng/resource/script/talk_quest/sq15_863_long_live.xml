   <talk_paragraph index="n710_scholar_hancock-3" image="QS_Default.dds" itemindex="300492" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n710_scholar_hancock-3-a" mid="32" select_type="0"><![CDATA[<html>What is this??</html>]]></talk_answer>
