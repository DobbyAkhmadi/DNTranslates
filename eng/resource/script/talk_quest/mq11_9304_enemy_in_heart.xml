   <talk_paragraph index="n520_angelica-1-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Angelica.dds" center_image="">
      <talk_answer link_index="n520_angelica-1-f" mid="385" select_type="2" />
      <talk_question mid="384"><![CDATA[<html>$Sorceress Angelica$<BR>Heheh. I do have an interesting story.<BR>Listen.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n520_angelica-1-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Angelica.dds" center_image="">
      <talk_answer link_index="n520_angelica-1-g" mid="388" select_type="2" />
      <talk_question mid="387"><![CDATA[<html>$Sorceress Angelica$<BR>You know that Black Knight is stirring up the whole Saint Haven right now. You must also know that wars need a lot of resources too.<BR>I am now in charge of preparing those resources.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n520_angelica-1-g" image="QN_Npc_Bishop.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Angelica.dds" center_image="">
      <talk_answer link_index="n520_angelica-1-h" mid="391" select_type="2" />
