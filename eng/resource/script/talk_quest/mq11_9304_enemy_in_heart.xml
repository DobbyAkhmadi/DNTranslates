   <talk_paragraph index="n520_angelica-1-j" image="QN_Npc_Bishop.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Angelica.dds" center_image="">
      <talk_answer link_index="n520_angelica-1-k" mid="399" select_type="2" />
