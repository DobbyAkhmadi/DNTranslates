   <talk_paragraph index="n1046_slave_david-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1046_slave_david-accepting-a" mid="6" select_type="0"><![CDATA[<html>Why me.</html>]]></talk_answer>
