   <talk_paragraph index="n1836_sarah-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1836_sarah-accepting-a" mid="37" select_type="0"><![CDATA[<html>Tell me what it's about.</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Merchant Sarrah$<BR>Sweet child, can you do me a favour this one last time?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1836_sarah-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1836_sarah-accepting-acceptted" mid="38" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
