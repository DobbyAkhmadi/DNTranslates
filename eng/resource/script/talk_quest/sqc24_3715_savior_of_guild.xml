   <talk_paragraph index="n2655_hongryun_ju-1-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2655_hongryun_ju-1-b" mid="17" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="16"><![CDATA[<html>$Head Merchant Jamihwa$<BR>No, I can’t ask someone to do this for me every time.<BR>I would like to ask you todo <FONT color=#086171> something else</FONT> for me.</html>]]></talk_question>
