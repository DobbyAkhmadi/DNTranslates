   <talk_paragraph index="n2655_hongryun_ju-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2655_hongryun_ju-1-a" mid="15" select_type="0"><![CDATA[<html>You still don’t have enough?</html>]]></talk_answer>
