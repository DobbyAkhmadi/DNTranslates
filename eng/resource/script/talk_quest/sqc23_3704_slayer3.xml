   <talk_paragraph index="n2638_cleric_aaron-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2638_cleric_aaron-accepting-a" mid="29" select_type="0"><![CDATA[<html>All right. Leave it to me. </html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Temple Knight Aaron$<BR>Now please use this entry ticket and eliminate darkness in Slayer Nest. Everyone has high hopes on you.</html>]]></talk_question>
