      <talk_answer link_index="n1723_teramai_encroach-5-" mid="55" select_type="5" />
      <talk_question mid="54"><![CDATA[<html>${user_nick}$<BR><FONT color=#7e12d2>(Now I should </FONT><FONT color=#7e12d2>go back to Eltia)</FONT></html>]]></talk_question>
