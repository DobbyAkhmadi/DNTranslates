   <talk_paragraph index="n2253_nophee-4" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Nophee.dds" center_image="">
      <talk_answer link_index="n2253_nophee-4-a" mid="74" select_type="2" />
      <talk_question mid="73"><![CDATA[<html>$Norfe$<BR><FONT color=#387206>{user_nick}</FONT>, what makes you run all the way here?<BR>Please catch your breath first…</html>]]></talk_question>
   </talk_paragraph>
