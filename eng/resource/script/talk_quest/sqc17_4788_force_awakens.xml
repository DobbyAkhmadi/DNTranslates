   <talk_paragraph index="n1823_merca_fargeau-1-e" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1823_merca_fargeau-1-f" mid="100" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
