   <talk_paragraph index="n091_trader_bellin-6" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n091_trader_bellin-6-a" mid="173" select_type="0"><![CDATA[<html>Do You feel better?</html>]]></talk_answer>
