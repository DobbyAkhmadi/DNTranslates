   <talk_paragraph index="n713_soceress_tamara-2-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n713_soceress_tamara-2-d" mid="65" select_type="0"><![CDATA[<html>(She's so scary)</html>]]></talk_answer>
