   <talk_paragraph index="n1737_eltia-4-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1737_eltia-4-g" mid="321" select_type="2" />
      <talk_question mid="267"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Until now... too many people died... to save me... Argh...<BR>When I saw Suriya... trying to sacrifice himself... to save the others... I finally understood...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1737_eltia-4-g" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1737_eltia-4-h" mid="322" select_type="2" />
