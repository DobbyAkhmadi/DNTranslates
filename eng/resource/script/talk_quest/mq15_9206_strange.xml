   <talk_paragraph index="n707_sidel-1-b" image="QN_warrior_barnak_silhouette.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-c" mid="52" select_type="2" />
      <talk_question mid="17"><![CDATA[<html>$Cidel$<BR><FONT color=#387206>{user_nick}</FONT>. I'll get ready to welcome Barnac. Can you do me a favour and bring him here?</html>]]></talk_question>
   </talk_paragraph>
