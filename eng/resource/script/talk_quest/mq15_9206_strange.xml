   <talk_paragraph index="n707_sidel-1-a" image="QN_warrior_barnak_silhouette.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-b" mid="51" select_type="2" />
