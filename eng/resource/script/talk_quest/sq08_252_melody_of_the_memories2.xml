   <talk_paragraph index="n026_trader_may-accepting" image="QS_Default.dds" itemindex="300165" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n026_trader_may-accepting-a" mid="72" select_type="0"><![CDATA[<html>(Merchant May flips the diary and shows you some of the entries.)</html>]]></talk_answer>
      <talk_question mid="71"><![CDATA[<html>$Merchant May$<BR>You are here at the right time. I looked through <FONT color=#a82424>Eureke's diary</FONT>. I think I know what sparked their argument.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n026_trader_may-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n026_trader_may-accepting-acceptted" mid="96" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
