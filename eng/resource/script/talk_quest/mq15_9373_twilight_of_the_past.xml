   <talk_paragraph index="n822_teramai-1" image="QN_Npc_Velskud1.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Teramai.dds" center_image="">
      <talk_answer link_index="n822_teramai-1-a" mid="98" select_type="2" />
      <talk_question mid="7"><![CDATA[<html>$Terrence Terramai$<BR>Phew... Velskud is no longer in a critical condition, but he needs to get treated continuously.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n822_teramai-1-a" image="QN_Npc_Velskud1.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Teramai.dds" center_image="">
      <talk_answer link_index="n822_teramai-2" mid="199" select_type="2" />
      <talk_question mid="198"><![CDATA[<html>$Terrence Terramai$<BR>We don't know what kind of side effects might appear without the dragon's jewel that had been preserving his life.<BR><FONT color=#7e12d2>(Terramai shakes his head)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n836_velskud-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Velskud.dds" center_image="">
      <talk_answer link_index="n836_velskud-1-" mid="100" select_type="5" />
