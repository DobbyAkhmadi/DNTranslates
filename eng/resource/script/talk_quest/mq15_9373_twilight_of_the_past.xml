   <talk_paragraph index="n822_teramai-4" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Teramai.dds" center_image="">
      <talk_answer link_index="n822_teramai-4-b" mid="163" select_type="2" />
      <talk_question mid="161"><![CDATA[<html>$Terrence Terramai$<BR>I have things to settle with Velskud, but I should concentrate on his recovery right now.</html>]]></talk_question>
