   <talk_paragraph index="n2045_merca_arc-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2045_merca_arc-accepting-a" mid="51" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Arc$<BR>I'm sorry we're behind schedule.<BR>Now you can awaken the power within you with one more thing.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2045_merca_arc-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2045_merca_arc-accepting-acceptted" mid="52" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
