   <talk_paragraph index="n1855_shaolong-1" image="QN_Npc_Lencea.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Shaolong.dds" center_image="">
      <talk_answer link_index="n1855_shaolong-1-a" mid="55" select_type="2" />
