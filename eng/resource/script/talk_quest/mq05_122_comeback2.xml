   <talk_paragraph index="n012_sorceress_master_cynthia-4" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n012_sorceress_master_cynthia-4-a" mid="40" select_type="0"><![CDATA[<html>(You hand over the box.)</html>]]></talk_answer>
      <talk_question mid="39"><![CDATA[<html>$Master Sorceress Cynthia$<BR>Yes? Have you found something?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n012_sorceress_master_cynthia-4-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n012_sorceress_master_cynthia-4-b" mid="42" select_type="0"><![CDATA[<html>How...how did that happen?!</html>]]></talk_answer>
      <talk_question mid="41"><![CDATA[<html>$Master Sorceress Cynthia$<BR>Are you kidding me? What is this?<BR><BR><FONT color=#7e12d2>(There is nothing inside the box!)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n012_sorceress_master_cynthia-4-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n012_sorceress_master_cynthia-4-c" mid="44" select_type="0"><![CDATA[<html>(I never thought it could be empty...)</html>]]></talk_answer>
      <talk_question mid="43"><![CDATA[<html>$Master Sorceress Cynthia$<BR>The monsters were smart enough to relocate the <FONT color=#a82424>Sage's Staff</FONT> to a different location, while you were stupid enough to deliver an empty box.</html>]]></talk_question>
   </talk_paragraph>
