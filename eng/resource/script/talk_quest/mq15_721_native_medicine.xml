   <talk_paragraph index="n701_oldkarakule-1-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-1-f" mid="112" select_type="2" />
      <talk_question mid="20"><![CDATA[<html>$Karacule Fairystar$<BR>Very well. We'll try it your way...if <FONT color=#387206>{user_nick}</FONT> can bring you the ingredients.</html>]]></talk_question>
