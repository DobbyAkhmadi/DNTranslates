   <talk_paragraph index="n701_oldkarakule-1-d" image="" itemindex="" portrait_left="NPC_Left_Sidel.dds" portrait_right="" center_image="">
      <talk_answer link_index="n701_oldkarakule-1-e" mid="111" select_type="2" />
      <talk_question mid="18"><![CDATA[<html>$Cidel$<BR>I can do this. I know I can! Trust me!</html>]]></talk_question>
