   <talk_paragraph index="n044_archer_master_ishilien-2-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n044_archer_master_ishilien-2-!next" mid="288" select_type="0"><![CDATA[<html><FONT color=#2b2723>(Why is she so surprised?)</FONT></html>]]></talk_answer>
      <talk_question mid="287"><![CDATA[<html>$Master Archer Ithilien$<BR>What? Lianna?<BR><FONT color=#7e12d2>(She seems flustered)</FONT></html>]]></talk_question>
