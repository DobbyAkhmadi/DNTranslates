   <talk_paragraph index="n2221_hound_free-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2221_hound_free-2-a" mid="60" select_type="0"><![CDATA[<html>(You can hear a hound barking from afar.)</html>]]></talk_answer>
      <talk_question mid="22"><![CDATA[<html>$Strange Hound$<BR>Woof!! Woof woof!! Woof woof woof woof wooooooof!!!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2221_hound_free-2-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2221_hound_free-2-b" mid="61" select_type="0"><![CDATA[<html>Stop barking, doggy!</html>]]></talk_answer>
