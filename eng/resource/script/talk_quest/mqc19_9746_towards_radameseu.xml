   <talk_paragraph index="n2036_merca_iris-1-c" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_iris.dds" center_image="">
      <talk_answer link_index="n2036_merca_iris-1-" mid="134" select_type="5" />
