   </talk_paragraph>
   <talk_paragraph index="n1047_argenta-5-c" image="" itemindex="" portrait_left="NPC_Left_geraint_Kid.dds" portrait_right="" center_image="">
      <talk_answer link_index="n1047_argenta-5-d" mid="71" select_type="2" />
      <talk_question mid="70"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Then why did the Dragon Followers...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1047_argenta-5-d" image="" itemindex="" portrait_left="NPC_Left_geraint_Kid.dds" portrait_right="" center_image="">
      <talk_answer link_index="n1047_argenta-5-e" mid="73" select_type="2" />
