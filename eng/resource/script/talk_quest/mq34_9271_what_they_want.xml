   <talk_paragraph index="n1047_argenta-1-a" image="QN_Npc_Geraint.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1047_argenta-1-b" mid="10" select_type="2" />
      <talk_question mid="9"><![CDATA[<html>$Argenta$<BR>I'm not thanking you though. <BR><FONT color=#a82424>Geraint</FONT> died because of you in the first place.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1047_argenta-1-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1047_argenta-1-c" mid="12" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>$Argenta$<BR>Anyway, that <FONT color=#a82424>Elf ranger over there</FONT> was looking for you.<BR>She tried to act like she was close to me. Imagine how irritated I was!!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1047_argenta-1-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1047_argenta-1-d" mid="130" select_type="2" />
      <talk_question mid="13"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>This place is full of elves. Who are you talking about?</FONT></html>]]></talk_question>
