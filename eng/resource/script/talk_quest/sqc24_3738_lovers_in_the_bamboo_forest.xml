   <talk_paragraph index="n2759_pale_ghost-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2759_pale_ghost-accepting-a" mid="6" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Cloudy Phantom$<BR>Even if I don't remember, you can tell just by looking at me.<BR>She isn't the type to come within a mile of the likes of me.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2759_pale_ghost-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
