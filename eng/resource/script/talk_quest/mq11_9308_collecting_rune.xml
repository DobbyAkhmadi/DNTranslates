   <talk_paragraph index="n518_cian-1" image="QS_Default.dds" itemindex="300020" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n518_cian-1-a" mid="849" select_type="2" />
      <talk_question mid="762"><![CDATA[<html>$Warrior Xian$<BR>To make Ancient Priest's Rune, we need to collect the <FONT color=#a82424>rune shards</FONT>.<BR>All the other adventurers have gone to find them.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n518_cian-1-a" image="QW_35.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n518_cian-1-b" mid="765" select_type="2" />
      <talk_question mid="764"><![CDATA[<html>$Warrior Xian$<BR>Rune shards can be found at the <FONT color=#a82424>Dried Marissa Spring</FONT>.<FONT color=#387206>{user_nick}</FONT>, I hope you can help out too.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n518_cian-1-b" image="QN_Npc_Scholar_Starshy.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n518_cian-2" mid="767" select_type="2" />
      <talk_question mid="766"><![CDATA[<html>$Warrior Xian$<BR>Please collect the rune shards there and take them to <FONT color=#a82424>Heraldry Scholar Stas</FONT>.<BR>We all agreed to meet there.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n518_cian-2" image="QN_NPC_Lunaria.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n518_cian-2-a" mid="769" select_type="2" />
