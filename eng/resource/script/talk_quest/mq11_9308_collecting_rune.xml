   <talk_paragraph index="n518_cian-4" image="QN_Npc_Scholar_Starshy.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n518_cian-4-" mid="814" select_type="5" />
      <talk_question mid="813"><![CDATA[<html>$Warrior Xian$<BR>Ah, you have brought the rune shards.<BR>Then please take them to <FONT color=#a82424> Heraldry Scholar Stas</FONT>.</html>]]></talk_question>
