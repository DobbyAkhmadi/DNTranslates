   <talk_paragraph index="n1047_argenta-1" image="QN_Npc_geraint_Kid.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1047_argenta-1-" mid="9" select_type="5" />
