   <talk_paragraph index="n886_lost_boy-2-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n886_lost_boy-2-d" mid="18" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="17"><![CDATA[<html>$Lost Boy$<BR>N-no.<BR><FONT color=#7e12d2>(The boy smiles shyly)<BR></FONT>Thanks to you I can go back home now.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n886_lost_boy-2-d" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n886_lost_boy-2-e" mid="20" select_type="0"><![CDATA[<html>What a funny boy.</html>]]></talk_answer>
      <talk_answer link_index="n886_lost_boy-2-e" mid="21" select_type="0"><![CDATA[<html>(Give him a noogie)</html>]]></talk_answer>
      <talk_answer link_index="n886_lost_boy-2-e" mid="22" select_type="0"><![CDATA[<html>It's not good to threaten people!</html>]]></talk_answer>
      <talk_question mid="19"><![CDATA[<html>$Lost Boy$<BR>This is a word of caution but it wouldn't be good if you follow me.</html>]]></talk_question>
