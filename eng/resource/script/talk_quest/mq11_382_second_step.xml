      <talk_answer link_index="n040_king_casius-2-h" mid="103" select_type="3" />
      <talk_question mid="102"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Do you know the whereabouts of the Black Dragon?</FONT></html>]]></talk_question>
   </talk_paragraph>
