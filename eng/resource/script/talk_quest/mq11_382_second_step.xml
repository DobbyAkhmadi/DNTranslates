   <talk_paragraph index="n393_academic_station-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Jasmin.dds" center_image="">
      <talk_answer link_index="n393_academic_station-1-a" mid="13" select_type="0"><![CDATA[<html>(Report what has been going on)</html>]]></talk_answer>
