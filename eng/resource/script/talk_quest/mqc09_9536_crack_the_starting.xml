   <talk_paragraph index="n1718_kaye_evil-3-d" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Kaye.dds" center_image="">
      <talk_answer link_index="n1718_kaye_evil-3-e" mid="81" select_type="2" />
