   <talk_paragraph index="n776_darklair_mardlen-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Darklair_Castia.dds" center_image="">
      <talk_answer link_index="n776_darklair_mardlen-1-a" mid="70" select_type="2" />
      <talk_question mid="10"><![CDATA[<html>$Priestess of Darkness Madeleine$<BR>You have finally regained your consciousness, <FONT color=#387206>{user_class}</FONT> <FONT color=#387206>{user_nick}</FONT>.<BR>Do you remember what happened at the Red Dragon Nest? </html>]]></talk_question>
