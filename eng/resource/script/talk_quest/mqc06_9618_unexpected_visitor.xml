   <talk_paragraph index="n465_charty-4-h" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Charty.dds" center_image="">
      <talk_answer link_index="n465_charty-4-i" mid="111" select_type="2" />
      <talk_question mid="46"><![CDATA[<html>$Charti$<BR>I'm going to explore Riverwort Village Ruins. My information source told me that there are signs that a lot of people passed by that place. </html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n465_charty-4-i" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Charty.dds" center_image="">
      <talk_answer link_index="n465_charty-4-j" mid="112" select_type="2" />
      <talk_question mid="47"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>But why would that witch want to kidnap your villagers?</FONT></html>]]></talk_question>
