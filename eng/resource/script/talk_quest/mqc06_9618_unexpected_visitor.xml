   </talk_paragraph>
   <talk_paragraph index="n465_charty-4-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Charty.dds" center_image="">
      <talk_answer link_index="n465_charty-4-c" mid="105" select_type="2" />
