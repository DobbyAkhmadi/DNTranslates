   <talk_paragraph index="n091_trader_bellin-1" image="QS_Default.dds" itemindex="300525" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n091_trader_bellin-2" mid="66" select_type="0"><![CDATA[<html>Got it.</html>]]></talk_answer>
      <talk_question mid="65"><![CDATA[<html>$Blacksmith Berlin$<BR>Anyway, the magnetic field inspection device is here.<BR>Take this and analyze it after staying at a place with monsters for an appropriate amount of time.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n036_adventurer_guildmaster_gunter-1" image="QN_Npc_Saint_BlackSmith_Dwarf.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
