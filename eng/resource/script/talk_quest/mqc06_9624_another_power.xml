   <talk_paragraph index="n725_charty-7-b" image="QN_Npc_Sidel.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Charty.dds" center_image="">
      <talk_answer link_index="n725_charty-7-c" mid="278" select_type="2" />
