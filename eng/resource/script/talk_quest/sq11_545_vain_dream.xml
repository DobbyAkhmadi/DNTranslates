   <talk_paragraph index="n045_soceress_master_stella-2-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n045_soceress_master_stella-2-d" mid="55" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
