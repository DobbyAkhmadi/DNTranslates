   <talk_paragraph index="n036_adventurer_guildmaster_gunter-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n036_adventurer_guildmaster_gunter-2-a" mid="32" select_type="0"><![CDATA[<html>(Tell him about Daisy)</html>]]></talk_answer>
