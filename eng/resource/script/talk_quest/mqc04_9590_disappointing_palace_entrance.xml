   <talk_paragraph index="n260_elite_soldier-3" image="" itemindex="" portrait_left="NPC_Left_Elite_Soilder.dds" portrait_right="" center_image="">
      <talk_answer link_index="n260_elite_soldier-3-a" mid="135" select_type="2" />
      <talk_question mid="33"><![CDATA[<html>$Elite Soldier Calrim$<BR>How did you come to Saint Haven…? Can hybrids even enter the Royal Castle? <BR>Do you have a pass?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n260_elite_soldier-3-a" image="" itemindex="" portrait_left="NPC_Left_Elite_Soilder.dds" portrait_right="" center_image="">
      <talk_answer link_index="n260_elite_soldier-3-b" mid="136" select_type="2" />
      <talk_question mid="34"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Here.</FONT></html>]]></talk_question>
