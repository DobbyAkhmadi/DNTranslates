   <talk_paragraph index="n099_engineer_hubert-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Engineer_Hubert.dds" center_image="">
      <talk_answer link_index="n099_engineer_hubert-1-d" mid="8" select_type="2" />
      <talk_question mid="7"><![CDATA[<html>$Airship Engineer Hubert$<BR>Great, I'll try using this engine battery that Lancea Charlotte brought back!</html>]]></talk_question>
