   <talk_paragraph index="n822_teramai-1" image="QW_61.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Teramai.dds" center_image="">
      <talk_answer link_index="n822_teramai-1-" mid="50" select_type="5" />
