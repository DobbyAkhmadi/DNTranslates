      <talk_answer link_index="n210_argenta-4-b" mid="64" select_type="2" />
      <talk_question mid="63"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Do you know that Blond-haired Swordsman?</FONT></html>]]></talk_question>
