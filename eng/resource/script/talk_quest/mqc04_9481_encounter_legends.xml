      <talk_answer link_index="n210_argenta-5-a" mid="74" select_type="2" />
      <talk_question mid="73"><![CDATA[<html>$Argenta$<BR>You shouldn't follow <FONT color=#a82424>Geraint</FONT>.</html>]]></talk_question>
