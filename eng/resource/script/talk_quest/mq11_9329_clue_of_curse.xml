   </talk_paragraph>
   <talk_paragraph index="n519_cian-1" image="QN_Npc_Triana.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Cian.dds" center_image="">
      <talk_answer link_index="n519_cian-1-a" mid="2883" select_type="2" />
