   <talk_paragraph index="n1147_lunaria-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1147_lunaria-1-" mid="2882" select_type="5" />
