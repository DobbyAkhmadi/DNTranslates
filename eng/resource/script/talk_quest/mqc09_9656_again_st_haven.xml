         <contents_image>QN_Npc_Velskud.dds</contents_image>
         <destination_pos>13014,n836</destination_pos>
         <destination_mode>custom:</destination_mode>
         <statemark_npcid>836</statemark_npcid>
         <statemark_type>2</statemark_type>
         <trigger_string />
         <maptool_index />
         <title mid="178"><![CDATA[Velskud]]></title>
