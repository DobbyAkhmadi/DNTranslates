   <talk_paragraph index="n707_sidel-2-a" image="QN_Npc_karahan.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2-b" mid="258" select_type="2" />
      <talk_question mid="215"><![CDATA[<html>$Cidel$<BR>I see. Karahan....<BR><FONT color=#7e12d2>(Cidel looks determined)</FONT></html>]]></talk_question>
   </talk_paragraph>
