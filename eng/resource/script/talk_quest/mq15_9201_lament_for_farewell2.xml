      <talk_question mid="240"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>You can tell me first.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n800_native_taruun-1-d" image="QN_Npc_Sidel.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_lotus_nativemanA.dds" center_image="">
