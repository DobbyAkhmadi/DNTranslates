   <talk_paragraph index="n707_sidel-2" image="QN_LM_Nativeman_A.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2-a" mid="257" select_type="2" />
      <talk_question mid="212"><![CDATA[<html>$Cidel${action:Talk}<BR>Oh! You're back, <FONT color=#387206>{user_nick}</FONT>.<BR>What did the native man say?</html>]]></talk_question>
