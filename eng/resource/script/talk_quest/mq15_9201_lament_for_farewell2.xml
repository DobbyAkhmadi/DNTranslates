   <talk_paragraph index="n707_sidel-accepting" image="QN_LM_Nativeman_A.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-accepting-acceptted" mid="231" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Cidel$<BR>The natives who escaped from the Dragon Followers are hiding somewhere. I don't know if I have the time for a funeral.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-1" image="QW_58.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-a" mid="232" select_type="2" />
