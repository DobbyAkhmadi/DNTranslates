   <talk_paragraph index="n1892_saint_guard-1-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Elite_Soilder.dds" center_image="">
      <talk_answer link_index="n1892_saint_guard-1-d" mid="72" select_type="2" />
