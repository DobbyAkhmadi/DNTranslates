   <talk_paragraph index="n1892_saint_guard-1-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Elite_Soilder.dds" center_image="">
      <talk_answer link_index="n1892_saint_guard-1-f" mid="74" select_type="2" />
