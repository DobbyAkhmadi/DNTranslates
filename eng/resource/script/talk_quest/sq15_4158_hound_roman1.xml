   <talk_paragraph index="n1006_hound_roman-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1006_hound_roman-1-a" mid="27" select_type="0"><![CDATA[<html>A hound? </html>]]></talk_answer>
