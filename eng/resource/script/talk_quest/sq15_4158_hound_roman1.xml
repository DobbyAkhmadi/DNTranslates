   <talk_paragraph index="n092_trader_kelly-2-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n092_trader_kelly-2-b" mid="57" select_type="0"><![CDATA[<html>What?</html>]]></talk_answer>
      <talk_question mid="56"><![CDATA[<html>$Merchant Kelly$<BR>I found out who took the hound food.</html>]]></talk_question>
