   <talk_paragraph index="n735_darklair_mocha-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n735_darklair_mocha-accepting-b" mid="8" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
