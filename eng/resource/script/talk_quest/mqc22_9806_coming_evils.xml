   <talk_paragraph index="n2350_holo_salica-2" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Salica.dds" center_image="">
      <talk_answer link_index="n2350_holo_salica-2-a" mid="78" select_type="2" />
