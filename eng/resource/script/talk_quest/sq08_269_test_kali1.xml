   <talk_paragraph index="n028_scholar_bailey-1-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n028_scholar_bailey-1-d" mid="130" select_type="0"><![CDATA[<html>I don't know him. Why should I receive that?</html>]]></talk_answer>
