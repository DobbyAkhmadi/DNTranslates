   <talk_paragraph index="n028_scholar_bailey-1-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n028_scholar_bailey-1-c" mid="128" select_type="0"><![CDATA[<html>A strange object?</html>]]></talk_answer>
