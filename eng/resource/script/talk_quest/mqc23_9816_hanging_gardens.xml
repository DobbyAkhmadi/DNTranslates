   <talk_paragraph index="n2254_geraint-7-f" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2254_geraint-7-!next" mid="89" select_type="2" />
      <talk_question mid="88"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>That’s right… I’m glad you’re back, Geraint. Can you take care of Grele while I look for Shalica?</FONT></html>]]></talk_question>
   </talk_paragraph>
