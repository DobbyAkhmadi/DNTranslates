   <talk_paragraph index="n875_academic-2-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Academic_Commelina.dds" center_image="">
      <talk_answer link_index="n875_academic-2-d" mid="168" select_type="2" />
