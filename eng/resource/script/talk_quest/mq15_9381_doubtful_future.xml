   <talk_paragraph index="n875_academic-2-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Academic_Commelina.dds" center_image="">
      <talk_answer link_index="n875_academic-2-c" mid="167" select_type="2" />
