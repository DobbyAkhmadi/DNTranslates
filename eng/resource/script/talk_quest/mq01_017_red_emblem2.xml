   <talk_paragraph index="n001_elder_harold-4-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Harold.dds" center_image="">
      <talk_answer link_index="n001_elder_harold-4-!next" mid="114" select_type="2" />
      <talk_question mid="113"><![CDATA[<html>$Mayor Harold$<BR>
Rose…. Where are you?<BR>
<FONT color=#7e12d2>(Tears swell up in Harold's eyes)</FONT></html>]]></talk_question>
