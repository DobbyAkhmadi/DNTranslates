   </talk_paragraph>
   <talk_paragraph index="n004_guard_steave-4-e" image="QM_HarpyRed.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Guard_Steave.dds" center_image="">
      <talk_answer link_index="n004_guard_steave-4-f" mid="114" select_type="2" />
