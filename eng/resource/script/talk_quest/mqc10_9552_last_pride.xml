   <talk_paragraph index="n1737_eltia-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1737_eltia-accepting-acceptted" mid="52" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Eltia$<BR>Stupid Kay... Enough with foolish mistakes. He should just come back….</html>]]></talk_question>
