   <talk_paragraph index="n1737_eltia-1-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1737_eltia-1-c" mid="55" select_type="2" />
      <talk_question mid="8"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I'll bring him back this time even if I have to put a leash on him!</FONT></html>]]></talk_question>
