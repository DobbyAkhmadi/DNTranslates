   <talk_paragraph index="n1729_kaye_arnut-2-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Kaye.dds" center_image="">
      <talk_answer link_index="n1729_kaye_arnut-2-c" mid="60" select_type="2" />
