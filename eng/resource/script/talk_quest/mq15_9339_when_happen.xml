      <talk_answer link_index="n701_oldkarakule-3-e" mid="77" select_type="2" />
      <talk_question mid="76"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Are you okay?</FONT></html>]]></talk_question>
