   <talk_paragraph index="n1067_elder_elf-6-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Elder_elf.dds" center_image="">
      <talk_answer link_index="n1067_elder_elf-6-c" mid="151" select_type="2" />
      <talk_question mid="56"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Yes it's me!</FONT><BR>
<FONT color=#662500>(I'm really back...!!!)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1067_elder_elf-6-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Elder_elf.dds" center_image="">
      <talk_answer link_index="n1067_elder_elf-6-" mid="152" select_type="5" />
      <talk_question mid="149"><![CDATA[<html><FONT color=#2b2723>$Elf Elder$</FONT><BR><FONT color=#2b2723>I thought you went to the Monolith... How did you end up in Anu Arendel...?</FONT><BR><FONT color=#7e12d2>(Elf Elder is suprised to see you. Wait for him to calm down before talking to him again)</FONT></html>]]></talk_question>
   </talk_paragraph>
