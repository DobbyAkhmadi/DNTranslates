   <talk_paragraph index="n1823_merca_fargeau-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1823_merca_fargeau-accepting-a" mid="101" select_type="0"><![CDATA[<html>Fargal's gone?</html>]]></talk_answer>
      <talk_question mid="100"><![CDATA[<html>$Merchant Fargeau$<BR><FONT color=#387206>{user_nick}</FONT>! <FONT color=#387206>{user_nick}</FONT>! Have you seen Fargal?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1823_merca_fargeau-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1823_merca_fargeau-accepting-b" mid="103" select_type="0"><![CDATA[<html>(Fargal is waving at you.)</html>]]></talk_answer>
      <talk_question mid="102"><![CDATA[<html>$Merchant Fargeau$<BR>Haha… Not really. Fargal has been desperately looking for you.<BR>I just happened to see you and that made me think of him. Oh, Fargal is right there!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1823_merca_fargeau-accepting-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
