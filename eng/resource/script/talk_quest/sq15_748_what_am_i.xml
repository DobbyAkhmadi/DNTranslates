   <talk_paragraph index="n796_academin_npc-1" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n796_academin_npc-1-a" mid="10" select_type="0"><![CDATA[<html>I'm not here because of you.</html>]]></talk_answer>
