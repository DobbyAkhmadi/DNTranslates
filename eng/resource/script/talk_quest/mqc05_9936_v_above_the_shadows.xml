   <talk_paragraph index="n3048_place-2-c" image="" itemindex="" portrait_left="Npc_Left_Kali.dds" portrait_right="" center_image="">
      <talk_answer link_index="n3048_place-2-d" mid="123" select_type="2" />
      <talk_question mid="15"><![CDATA[<html>$Kali$<BR>… Grele. That's… </html>]]></talk_question>
