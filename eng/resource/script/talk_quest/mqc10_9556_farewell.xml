   <talk_paragraph index="n822_teramai-6-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Teramai.dds" center_image="">
      <talk_answer link_index="n822_teramai-6-d" mid="200" select_type="2" />
