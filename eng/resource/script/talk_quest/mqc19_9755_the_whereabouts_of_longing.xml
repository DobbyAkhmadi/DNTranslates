   <talk_paragraph index="n2065_grele-accepting" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_child_grele.dds" center_image="">
      <talk_answer link_index="n2065_grele-accepting-acceptted" mid="508" select_type="3" />
      <talk_question mid="439"><![CDATA[<html>$Grele$<BR>Hiver?? You have my sister's news? Is she all right??</html>]]></talk_question>
