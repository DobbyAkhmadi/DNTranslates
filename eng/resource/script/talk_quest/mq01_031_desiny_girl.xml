   <talk_paragraph index="n001_elder_harold-2" image="QW_1.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Harold.dds" center_image="">
      <talk_answer link_index="n001_elder_harold-2-b" mid="202" select_type="0"><![CDATA[<html>Can I get some adaptation training?</html>]]></talk_answer>
      <talk_answer link_index="n001_elder_harold-2-a" mid="203" select_type="0"><![CDATA[<html>I'll be back in a while</html>]]></talk_answer>
      <talk_question mid="159"><![CDATA[<html>$Mayor Harold$<BR>Please go to the <FONT color=#a82424>Shadow Forest Trail</FONT> and get the Shadow Forest Dandelion.<BR>You can find it at the deepest part of the forest. Please be careful on your way.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n001_elder_harold-2-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Harold.dds" center_image="">
      <talk_answer link_index="n001_elder_harold-2-" mid="205" select_type="5" />
      <talk_question mid="204"><![CDATA[<html>$Mayor Harold$<BR>You are a good <FONT color=#387206>{user_class}</FONT>. If you need a <FONT color=#a82424>[Detailed Tutorial]</FONT>, please talk to me anytime.<BR>Adaptation trainings are very important….</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n001_elder_harold-2-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Harold.dds" center_image="">
      <talk_answer link_index="n001_elder_harold-2-party1" mid="207" select_type="0"><![CDATA[<html>Yes, I'll take it now.</html>]]></talk_answer>
      <talk_answer link_index="n001_elder_harold-2-e" mid="208" select_type="0"><![CDATA[<html>No thank you.</html>]]></talk_answer>
      <talk_question mid="206"><![CDATA[<html>$Mayor Harold$<BR>Really? Yes, adaptation training is very important….<BR>May I guide you to the <FONT color=#a82424>[Detailed Tutorial]</FONT>?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n001_elder_harold-2-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Harold.dds" center_image="">
      <talk_answer link_index="n001_elder_harold-2-" mid="210" select_type="5" />
      <talk_question mid="209"><![CDATA[<html>$Mayor Harold$<BR>I'm sorry. Only <FONT color=#a82424>one</FONT> person can enter the tutorial.<BR>
You can always take it next time by selecting <FONT color=#a82424>[Get Tutorial]</FONT> after talking to me.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n001_elder_harold-2-d" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Harold.dds" center_image="">
      <talk_answer link_index="n001_elder_harold-2-move_harold" mid="212" select_type="2" />
      <talk_question mid="211"><![CDATA[<html>$Mayor Harold$<BR>Now, I will send you to a special training ground for new adventurers.<BR>Good luck.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n001_elder_harold-2-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Harold.dds" center_image="">
      <talk_answer link_index="n001_elder_harold-2-f" mid="214" select_type="2" />
