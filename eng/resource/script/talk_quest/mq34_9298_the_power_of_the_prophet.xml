   <talk_paragraph index="n1133_rojalin-3-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Rojalin.dds" center_image="">
      <talk_answer link_index="n1133_rojalin-3-b" mid="26" select_type="2" />
