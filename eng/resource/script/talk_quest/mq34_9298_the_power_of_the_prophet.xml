   <talk_paragraph index="n1133_rojalin-3" image="QN_Npc_yubaensiael.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Rojalin.dds" center_image="">
      <talk_answer link_index="n1133_rojalin-3-a" mid="24" select_type="2" />
