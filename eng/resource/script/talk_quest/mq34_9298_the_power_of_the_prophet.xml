   <talk_paragraph index="n1034_argenta-4-d" image="QN_Npc_yubaensiael.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1034_argenta-4-e" mid="48" select_type="2" />
      <talk_question mid="47"><![CDATA[<html>$Argenta$<BR>I feel like punching Yuvenciel in the face for making things turn out like this but<BR>where did the two elves go…?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1034_argenta-4-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1034_argenta-4-f" mid="50" select_type="2" />
