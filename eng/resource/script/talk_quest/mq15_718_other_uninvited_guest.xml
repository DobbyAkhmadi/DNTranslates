      <talk_answer link_index="n701_oldkarakule-4-b" mid="209" select_type="2" />
      <talk_question mid="208"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Tell her you met Velskud.)</FONT></html>]]></talk_question>
