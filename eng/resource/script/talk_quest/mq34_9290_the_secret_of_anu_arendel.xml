   <talk_paragraph index="n1047_argenta-accepting" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1047_argenta-accepting-!next" mid="6" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Argenta$<BR>He's not the Geraint we used to know. We are beings born purely from the jewel's power.<BR>No old memories or feeling remain for him after being newly born again.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1065_geraint_kid-1" image="QN_Npc_Argenta.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_geraint_Kid.dds" center_image="">
      <talk_answer link_index="n1065_geraint_kid-1-" mid="9" select_type="5" />
