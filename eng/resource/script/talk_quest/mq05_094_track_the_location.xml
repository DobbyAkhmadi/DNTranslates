   <talk_paragraph index="n012_sorceress_master_cynthia-1-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_SoceressCynthia.dds" center_image="">
      <talk_answer link_index="n012_sorceress_master_cynthia-1-g" mid="81" select_type="2" />
