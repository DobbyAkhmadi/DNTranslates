   <talk_paragraph index="n024_adventurer_guildmaster_decud-2" image="QW_20.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Master_Decud.dds" center_image="">
      <talk_answer link_index="n024_adventurer_guildmaster_decud-2-" mid="54" select_type="5" />
      <talk_question mid="14"><![CDATA[<html>$Adventurer's Guild Master Deckard$<BR>According to the information gathered, this elf girl is one tough cookie. Be careful. Make sure you search for her at <FONT color=#a82424>Catacombs Hall</FONT> in Gray Ruins.</html>]]></talk_question>
