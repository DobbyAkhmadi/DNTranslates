   <talk_paragraph index="n1737_eltia-1-f" image="QN_Npc_Velskud1.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1737_eltia-1-g" mid="22" select_type="2" />
