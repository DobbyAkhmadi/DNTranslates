   <talk_paragraph index="n1744_general_duglars-6-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n1744_general_duglars-5" mid="128" select_type="2" />
      <talk_question mid="127"><![CDATA[<html>$General Douglas$<BR>Hahaha! Don't worry. You saw him fall to his death too.<BR>The Prophet is now with the silver-haired swordswoman who is helping you. Everything is perfect! You've played a major role in this victory. I'm sure His Highness would reward you generously!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7" image="" itemindex="" portrait_left="NPC_Left_Argenta.dds" portrait_right="" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-a" mid="61" select_type="2" />
      <talk_question mid="60"><![CDATA[<html>$Argenta$<BR>Geraint! Geraint! Wake up!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-b" mid="63" select_type="2" />
      <talk_question mid="62"><![CDATA[<html>$Geraint$<BR>(Coughs) I'm fine, Argenta.<BR>You don't have to... worry...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-b" image="" itemindex="" portrait_left="NPC_Left_Argenta.dds" portrait_right="" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-c" mid="65" select_type="2" />
      <talk_question mid="64"><![CDATA[<html>$Argenta$<BR>How can I not worry!<BR>Your other eye is… Now you can't see anything!<BR><FONT color=#7e12d2>(Argenta cries with Geraint in her arms)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-d" mid="67" select_type="2" />
      <talk_question mid="66"><![CDATA[<html>$Geraint$<BR>We did our best.<BR>You don't have to blame yourself and you shouldn't.<BR>You did your best, Argenta….</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-d" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-e" mid="69" select_type="2" />
      <talk_question mid="68"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Geraint……</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-f" mid="71" select_type="2" />
      <talk_question mid="70"><![CDATA[<html>$Geraint${action:Talk}<BR>I can't see….<BR>I was hit by the Black Dragon's breath right on my face, so it would be hard for me to regain my vision anytime soon….</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-g" mid="73" select_type="2" />
      <talk_question mid="72"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>No!! I'll find a way to fix your eyes.</FONT><BR>
<FONT color=#662500>And find the Black Knight...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-g" image="" itemindex="" portrait_left="NPC_Left_Argenta.dds" portrait_right="" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-h" mid="75" select_type="2" />
      <talk_question mid="74"><![CDATA[<html>$Argenta$<BR>Shut up! You can't do anything, human.<BR>Geraint, you fool…</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-h" image="QW_SaintHaven_Inside.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Geraint_Closeeye.dds" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-i" mid="77" select_type="2" />
      <talk_question mid="76"><![CDATA[<html>$Geraint$<BR>Argenta… We have to… take the Ancient girl to... Saint Haven Royal Castle.<BR>I'll create a nest there.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n214_geraint_wounded-7-i" image="" itemindex="" portrait_left="NPC_Left_Argenta.dds" portrait_right="" center_image="">
      <talk_answer link_index="n214_geraint_wounded-7-j" mid="157" select_type="2" />
      <talk_question mid="78"><![CDATA[<html>$Argenta$<BR>You can just eat her and be done with this!<BR><FONT color=#7e12d2>(Argenta takes Geraint and the girl away)</FONT></html>]]></talk_question>
