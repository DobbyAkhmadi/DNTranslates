   </talk_paragraph>
   <talk_paragraph index="n1575_gaharam-5-f" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1575_gaharam-5-g" mid="88" select_type="2" />
      <talk_question mid="40"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>(Is this Gaharam the one who brought me here?!)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1575_gaharam-5-g" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
