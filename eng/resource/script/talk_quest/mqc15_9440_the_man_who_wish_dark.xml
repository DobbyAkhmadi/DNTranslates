      <talk_answer link_index="n1575_gaharam-5-a" mid="80" select_type="2" />
      <talk_question mid="34"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I'm sure that was... Lunaria...</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1575_gaharam-5-a" image="QN_NPC_Lunaria.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
