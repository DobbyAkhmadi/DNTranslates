   </talk_paragraph>
   <talk_paragraph index="n1575_gaharam-5-c" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1575_gaharam-5-d" mid="84" select_type="2" />
      <talk_question mid="37"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Come on, tell me what is happening!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1575_gaharam-5-d" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Gaharam.dds" center_image="">
      <talk_answer link_index="n1575_gaharam-5-e" mid="86" select_type="2" />
