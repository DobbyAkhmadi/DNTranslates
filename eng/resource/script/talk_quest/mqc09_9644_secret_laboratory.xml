   <talk_paragraph index="n1914_cleric_chris-3" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Chris.dds" center_image="">
      <talk_answer link_index="n1914_cleric_chris-3-" mid="428" select_type="5" />
