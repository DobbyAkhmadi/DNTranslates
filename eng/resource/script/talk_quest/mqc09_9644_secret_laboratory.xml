   <talk_paragraph index="n1914_cleric_chris-2-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Chris.dds" center_image="">
      <talk_answer link_index="n1914_cleric_chris-3" mid="427" select_type="2" />
