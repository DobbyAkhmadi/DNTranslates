   <talk_paragraph index="n451_robinson-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n451_robinson-2-a" mid="75" select_type="0"><![CDATA[<html>Do you have something to be worried about?</html>]]></talk_answer>
      <talk_question mid="74"><![CDATA[<html>$Poor Kid$<BR>Ahh... What should I do? Damn....<BR>I feel so heavy.</html>]]></talk_question>
