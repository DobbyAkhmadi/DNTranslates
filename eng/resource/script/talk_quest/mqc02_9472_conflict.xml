   </talk_paragraph>
   <talk_paragraph index="n1690_eltia-5-c" image="QW_SaintHaven.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1690_eltia-5-!next" mid="88" select_type="2" />
