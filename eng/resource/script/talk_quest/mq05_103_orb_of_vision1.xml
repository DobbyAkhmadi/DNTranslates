   <talk_paragraph index="n012_sorceress_master_cynthia-1-a" image="QS_Default.dds" itemindex="300059" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n012_sorceress_master_cynthia-1-b" mid="12" select_type="0"><![CDATA[<html>(.........)</html>]]></talk_answer>
      <talk_question mid="9"><![CDATA[<html>$Master Sorceress Cynthia$<BR>Why do you ask about the <FONT color=#a82424>Vision Orb</FONT>?<BR>Is it because you stupid clerics lost your <FONT color=#a82424>Sage's Staff</FONT> to the monsters?</html>]]></talk_question>
