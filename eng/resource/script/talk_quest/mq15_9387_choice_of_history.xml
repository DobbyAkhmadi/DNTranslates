   <talk_paragraph index="n836_velskud-2-n" image="" itemindex="" portrait_left="NPC_Left_Academic_Commelina.dds" portrait_right="NPC_Right_Velskud.dds" center_image="">
      <talk_answer link_index="n836_velskud-2-o" mid="109" select_type="2" />
