   <talk_paragraph index="n1015_academic-5-j" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Academic_Commelina.dds" center_image="">
      <talk_answer link_index="n1015_academic-5-k" mid="105" select_type="2" />
      <talk_question mid="104"><![CDATA[<html>$Academic Comelina$<BR>Well...<BR>Seeing a person run is a <FONT color=#a82424>common thing</FONT>.<BR><FONT color=#7e12d2>(Comelina nods forcefully)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1015_academic-5-k" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Academic_Commelina.dds" center_image="">
      <talk_answer link_index="n1015_academic-5-l" mid="107" select_type="2" />
      <talk_question mid="106"><![CDATA[<html>${user_nick}$<BR>But even for people who look the same?</html>]]></talk_question>
