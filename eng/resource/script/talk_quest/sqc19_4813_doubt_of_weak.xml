   <talk_paragraph index="n2063_sorceress_anna-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2063_sorceress_anna-accepting-a" mid="645" select_type="0"><![CDATA[<html>No, not really…</html>]]></talk_answer>
      <talk_question mid="593"><![CDATA[<html>$Sorceress Anna$<BR>Sigh... I keep thinking of that little kid. Hey, <FONT color=#387206>{user_nick}</FONT>, are you good with kids?</html>]]></talk_question>
   </talk_paragraph>
