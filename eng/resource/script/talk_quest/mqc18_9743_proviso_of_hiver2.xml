      <talk_answer link_index="n2032_merca_grele-3-b" mid="69" select_type="2" />
      <talk_question mid="29"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>What are you doing here, Grele?!</FONT></html>]]></talk_question>
   </talk_paragraph>
