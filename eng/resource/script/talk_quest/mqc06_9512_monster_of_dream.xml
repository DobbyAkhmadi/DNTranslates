   <talk_paragraph index="n701_oldkarakule-2-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-2-d" mid="95" select_type="2" />
      <talk_question mid="15"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Karahan? Wow is he really your brother?!</FONT></html>]]></talk_question>
