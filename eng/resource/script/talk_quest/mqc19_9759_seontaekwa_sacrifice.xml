   <talk_paragraph index="n2089_grele-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_child_grele.dds" center_image="">
      <talk_answer link_index="n2089_grele-1-" mid="640" select_type="5" />
      <talk_question mid="575"><![CDATA[<html>$Grele$<BR>I'm going to save those who are hurt. </html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2091_refugee_girl-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_girl.dds" center_image="">
