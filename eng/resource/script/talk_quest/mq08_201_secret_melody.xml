   <talk_paragraph index="n024_adventurer_guildmaster_decud-2-c" image="QN_Rose.dds" itemindex="" portrait_left="NPC_Left_Master_Decud.dds" portrait_right="NPC_Right_Master_Decud.dds" center_image="">
      <talk_answer link_index="n024_adventurer_guildmaster_decud-2-d" mid="145" select_type="2" />
