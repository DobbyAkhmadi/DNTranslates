   <talk_paragraph index="n1101_lunaria-5-j" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1101_lunaria-5-k" mid="1548" select_type="0"><![CDATA[<html>Yeah I thought so.</html>]]></talk_answer>
      <talk_answer link_index="n1101_lunaria-5-k" mid="1549" select_type="0"><![CDATA[<html>You can't tell me anything.</html>]]></talk_answer>
