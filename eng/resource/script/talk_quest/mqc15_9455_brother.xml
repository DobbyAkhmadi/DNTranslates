      <talk_answer link_index="n1503_gereint_kid-2-f" mid="105" select_type="2" />
      <talk_question mid="95"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I'm here now. Why don't you believe me when I'm standing right before your eyes?</FONT></html>]]></talk_question>
