   <talk_paragraph index="n1501_argenta-2-q" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1501_argenta-2-r" mid="75" select_type="2" />
      <talk_question mid="26"><![CDATA[<html>$Argenta${sound:npc_Argenta_9455_qu_11.ogg}<BR>I might as well take this corrupt jewel with me on my way.<BR>I should disappear with whatever I can take with me.</html>]]></talk_question>
