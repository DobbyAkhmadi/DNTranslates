   <talk_paragraph index="n1501_argenta-2-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1501_argenta-2-c" mid="57" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>$Argenta${sound:npc_Argenta_9455_qu_02.ogg}<BR>Didn't that darn Ancient's jewel change you at least a little? You still look as irritating as before.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1501_argenta-2-c" image="" itemindex="" portrait_left="Npc_Left_Velskud_Human.dds" portrait_right="" center_image="">
