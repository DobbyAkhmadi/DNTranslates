   <talk_paragraph index="n1503_gereint_kid-2-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_geraint_Kid.dds" center_image="">
      <talk_answer link_index="n1503_gereint_kid-2-c" mid="102" select_type="2" />
