      <talk_answer link_index="n1503_gereint_kid-2-d" mid="103" select_type="2" />
      <talk_question mid="93"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>What are you talking about? I tried everything I could to come back here.</FONT></html>]]></talk_question>
