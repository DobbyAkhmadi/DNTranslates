      <talk_answer link_index="n1501_argenta-2-q" mid="74" select_type="2" />
      <talk_question mid="25"><![CDATA[<html>$Velskud${sound:npc_Velskud_9455_qu_05.ogg}<BR>...!!</html>]]></talk_question>
