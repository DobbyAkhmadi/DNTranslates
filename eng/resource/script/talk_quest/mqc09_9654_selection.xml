   <talk_paragraph index="n707_sidel-1" image="QN_LM_Nativeman_B.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-a" mid="8" select_type="2" />
      <talk_question mid="7"><![CDATA[<html>$Cidel$<BR>Suriya took the <FONT color=#a82424>meteor core fragment</FONT> and the <FONT color=#a82424>purification device</FONT> from me. <BR>I don't know what he might do so I hope you can go and talk to him, <FONT color=#387206>{user_nick}</FONT>.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-1-a" image="QN_LM_Nativeman_B.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-b" mid="10" select_type="2" />
      <talk_question mid="9"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Did you... ask Suriya to do that?</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-1-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-2" mid="12" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>$Cidel$<BR>No, never. I didn't say anything to him. It's something Suriya decided for himself.<BR>But I must say I agree with his decision.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1925_suriya-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Suriya.dds" center_image="">
      <talk_answer link_index="n1925_suriya-1-" mid="14" select_type="5" />
      <talk_question mid="13"><![CDATA[<html>$Suriya$<BR>So this is the purification device.</html>]]></talk_question>
   </talk_paragraph>
