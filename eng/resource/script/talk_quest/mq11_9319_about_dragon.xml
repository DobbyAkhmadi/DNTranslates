   <talk_paragraph index="n1155_lunaria-4-b" image="QN_Npc_Geraint.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1155_lunaria-4-c" mid="1916" select_type="2" />
      <talk_question mid="1915"><![CDATA[<html>$Lunaria$<BR>Dragons?<BR>Geraint and Argenta? Or the Black Knight?</html>]]></talk_question>
