   <talk_paragraph index="n707_sidel-accepting-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-accepting-acceptted" mid="634" select_type="3" />
      <talk_question mid="633"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>But you don't even know where she is going!</FONT></html>]]></talk_question>
