      <talk_answer link_index="n836_velskud-1-h" mid="99" select_type="2" />
      <talk_question mid="98"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>What…? My father is an… Apostle?!</FONT></html>]]></talk_question>
