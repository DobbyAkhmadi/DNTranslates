   <talk_paragraph index="n701_oldkarakule-accepting" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-accepting-acceptted" mid="36" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Karacule Fairystar$<BR>You're back earlier than I expected.</html>]]></talk_question>
