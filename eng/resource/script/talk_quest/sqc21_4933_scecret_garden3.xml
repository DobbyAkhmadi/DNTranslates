   <talk_paragraph index="n2258_farghal-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2258_farghal-accepting-a" mid="323" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="296"><![CDATA[<html>$Alchemist Fargal$<BR>Since we're on this anyway, why don't we investigate a little more?<BR>Good, I think we're getting along well.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2258_farghal-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2258_farghal-accepting-acceptted" mid="324" select_type="0"><![CDATA[<html><FONT color=#7e12d2>(Accept Quest)</FONT></html>]]></talk_answer>
