   <talk_paragraph index="n2259_hector-1-i" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2259_hector-1-j" mid="29" select_type="0"><![CDATA[<html>Mona, tell us which fish was stolen today.</html>]]></talk_answer>
      <talk_question mid="28"><![CDATA[<html>$Captain Hector$<BR>I told you! My bird would never do such a thing!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2259_hector-1-j" image="QN_Npc_Woman_Present_Amelia_Merca.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2259_hector-1-k" mid="31" select_type="0"><![CDATA[<html>What about yesterday?</html>]]></talk_answer>
      <talk_question mid="30"><![CDATA[<html>$Fish Seller Mona$<BR>I got my <FONT color=#a82424>golden halibuts</FONT> stolen today.</html>]]></talk_question>
