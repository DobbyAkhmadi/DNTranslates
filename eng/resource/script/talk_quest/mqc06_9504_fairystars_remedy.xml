   <talk_paragraph index="n707_sidel-1-e" image="QS_Default.dds" itemindex="300379" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-f" mid="128" select_type="2" />
      <talk_question mid="70"><![CDATA[<html>$Cidel$<BR>My friend loves <FONT color=#a82424>Jungle Bracken</FONT>. So I promised to give her some today, but I just have too many things on my hand right now. </html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-1-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-g" mid="129" select_type="2" />
      <talk_question mid="71"><![CDATA[<html>$Cidel$<BR>I'm sorry but do you mind getting some <FONT color=#a82424>Jungle Bracken</FONT> for me?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-1-g" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-1-h" mid="130" select_type="2" />
      <talk_question mid="72"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Sure! That would be easy! <BR></FONT><FONT color=#662500>…Erm… But where can I find this Jungle Bracken…? Which way should I go?</FONT><FONT color=#662500> </FONT></html>]]></talk_question>
   </talk_paragraph>
