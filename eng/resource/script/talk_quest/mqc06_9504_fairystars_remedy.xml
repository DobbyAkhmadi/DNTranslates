   <talk_paragraph index="n1692_kaye_riverwart-2-g" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Kaye.dds" center_image="">
      <talk_answer link_index="n1692_kaye_riverwart-2-h" mid="140" select_type="2" />
      <talk_question mid="87"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Wow am I that great now? Even Kay praised me! Woohoo!<BR>(Kay seems to think you're pathetic but don't care about him)<BR></FONT></html>]]></talk_question>
