   <talk_paragraph index="n088_scholar_starshy-1-k" image="QM_Manticore_black.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n088_scholar_starshy-1-l" mid="56" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="55"><![CDATA[<html>$Heraldry Scholar Stas$<BR>
