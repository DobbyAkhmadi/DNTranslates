   <talk_paragraph index="n042_general_duglars-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n042_general_duglars-accepting-a" mid="35" select_type="0"><![CDATA[<html>There's something I have to ask you...</html>]]></talk_answer>
      <talk_question mid="34"><![CDATA[<html>$General Douglas$<BR><FONT color=#387206>{user_nick}?</FONT> What are you doing here? You look like there's something bothering you.</html>]]></talk_question>
