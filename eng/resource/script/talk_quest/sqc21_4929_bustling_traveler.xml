   <talk_paragraph index="n1823_merca_fargeau-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1823_merca_fargeau-accepting-a" mid="193" select_type="0"><![CDATA[<html>Fargeau, what's the matter?</html>]]></talk_answer>
      <talk_question mid="154"><![CDATA[<html>$Merchant Fargeau$<BR>Ugh, what should I do now? Fargal is always so careless…….</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1823_merca_fargeau-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1823_merca_fargeau-accepting-acceptted" mid="194" select_type="0"><![CDATA[<html><FONT color=#7e12d2>(Accept Quest)</FONT></html>]]></talk_answer>
