   </talk_paragraph>
   <talk_paragraph index="n2725_norphee-2-c" image="" itemindex="" portrait_left="Npc_Left_Nophee.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2725_norphee-3" mid="74" select_type="2" />
