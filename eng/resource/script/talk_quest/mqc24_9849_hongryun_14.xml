      <talk_question mid="13"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I think the head merchant</FONT><FONT color=#662500> really trusts us, huh?</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2725_norphee-2-a" image="" itemindex="" portrait_left="Npc_Left_Nophee.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2725_norphee-2-b" mid="72" select_type="2" />
