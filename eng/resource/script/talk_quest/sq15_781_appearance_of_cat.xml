   <talk_paragraph index="n722_cat_mewmew-1" image="QW_52.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_question mid="15"><![CDATA[<html>$Multi-Purpose Cat$<BR>Now go to <FONT color=#a82425>Riverwort Village Ruins</FONT> and beat the monsters up.<BR>Let's go in hurry! The laundry's going to get bad smell.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n722_cat_mewmew-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n722_cat_mewmew-2-a" mid="17" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
