   <talk_paragraph index="n801_sorceress_tessa-1" image="QN_Npc_OldKarakule.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Sorceress_Tessa" center_image="">
      <talk_answer link_index="n801_sorceress_tessa-1-" mid="17" select_type="5" />
      <talk_question mid="16"><![CDATA[<html>$Sorceress Tysha$<BR>I can't believe that Master Karacule passed away....</html>]]></talk_question>
