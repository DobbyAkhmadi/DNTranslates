   <talk_paragraph index="n707_sidel-4-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-4-b" mid="73" select_type="2" />
      <talk_question mid="72"><![CDATA[<html>$Cidel$<BR>I see… So she's going to send the official letters too...<BR>I'm so glad to have her with me.</html>]]></talk_question>
   </talk_paragraph>
