   <talk_paragraph index="n707_sidel-4" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n707_sidel-4-a" mid="71" select_type="2" />
      <talk_question mid="70"><![CDATA[<html>$Cidel$<BR>Did you see Tamara?</html>]]></talk_question>
