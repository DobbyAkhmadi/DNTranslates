   <talk_paragraph index="n1154_lunaria-4-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1154_lunaria-4-g" mid="1385" select_type="0"><![CDATA[<html>…Huh? What's wrong with Lunaria today?</html>]]></talk_answer>
      <talk_answer link_index="n1154_lunaria-4-g" mid="1386" select_type="0"><![CDATA[<html>(Huh...?)</html>]]></talk_answer>
