   <talk_paragraph index="n575_shadow_meow-1" image="QW_Karakule_House.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Shadow_Meow.dds" center_image="">
      <talk_answer link_index="n575_shadow_meow-1-" mid="10" select_type="5" />
