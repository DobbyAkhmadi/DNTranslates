   <talk_paragraph index="n1232_white_dragon-accepting" image="QN_Npc_Lotus_Elder_Teramai.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_White_Dragon.dds" center_image="">
      <talk_answer link_index="n1232_white_dragon-accepting-acceptted_1" mid="73" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$White Dragon Iona$<BR>Are you going to the Royal Castle to meet Terramai to hear about the last Black Dragon Raid story?<BR>I'll wait for you here… Sorry I can't go with you.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1234_white_dragon-accepting" image="QN_Npc_Lotus_Elder_Teramai.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_White_Dragon.dds" center_image="">
