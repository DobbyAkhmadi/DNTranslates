   <talk_paragraph index="n1701_teramai_east-3-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Teramai.dds" center_image="">
      <talk_answer link_index="n1701_teramai_east-3-c" mid="74" select_type="2" />
      <talk_question mid="24"><![CDATA[<html>$Terrence Terramai$<BR><FONT color=#7e12d2>(Terramai looks embarrassed but smiles)</FONT><BR>We need to find ways to wake the Prophet up, and find out what the Apostles want from this place.</html>]]></talk_question>
