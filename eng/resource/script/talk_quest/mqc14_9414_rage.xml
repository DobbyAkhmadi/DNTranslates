   </talk_paragraph>
   <talk_paragraph index="n1518_for_memory-4-a" image="" itemindex="" portrait_left="NPC_Left_Arno.dds" portrait_right="" center_image="">
      <talk_answer link_index="n1518_for_memory-4-b" mid="42" select_type="2" />
