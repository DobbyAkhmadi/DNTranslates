   <talk_paragraph index="n1518_for_memory-4" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1518_for_memory-4-a" mid="40" select_type="2" />
