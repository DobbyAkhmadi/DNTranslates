   <talk_paragraph index="n1518_for_memory-4-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1518_for_memory-4-c" mid="44" select_type="2" />
      <talk_question mid="43"><![CDATA[<html>$Argenta$<BR>No matter how corrupted you've become, you still have your brain! Don't you know that what you just did can destroy this world?<BR>How dare you open the Monolith!!!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1518_for_memory-4-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Argenta.dds" center_image="">
      <talk_answer link_index="n1518_for_memory-4-d" mid="46" select_type="2" />
