      <talk_answer link_index="n1686_eltia-1-g" mid="69" select_type="2" />
      <talk_question mid="17"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Why are you just sitting there? We need to find father!</FONT></html>]]></talk_question>
