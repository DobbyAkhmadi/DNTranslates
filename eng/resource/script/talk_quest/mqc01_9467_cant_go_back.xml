   </talk_paragraph>
   <talk_paragraph index="n1686_eltia-1-r" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1686_eltia-1-s" mid="82" select_type="2" />
