      <talk_answer link_index="n2306_iris-1-d" mid="16" select_type="2" />
      <talk_question mid="15"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I was hoping he might appear again... I guess not...</FONT></html>]]></talk_question>
