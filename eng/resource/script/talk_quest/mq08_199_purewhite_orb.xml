   <talk_paragraph index="n1131_lunaria-3" image="QS_Default.dds" itemindex="300064" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1131_lunaria-3-a" mid="1381" select_type="2" />
      <talk_question mid="1380"><![CDATA[<html>$Lunaria$<BR>You've obtained the White Purity Orb.<BR>A fragment of it to be exact.<BR><FONT color=#7e12d2>(Lunaria picks up a fragment of the broken White Purity Orb)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1131_lunaria-3-a" image="QS_Default.dds" itemindex="300064" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1131_lunaria-3-b" mid="1383" select_type="2" />
      <talk_question mid="1382"><![CDATA[<html>${user_nick}$<BR>We need this orb to purify Calderock...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1131_lunaria-3-b" image="QS_Default.dds" itemindex="300064" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1131_lunaria-3-c" mid="1388" select_type="0"><![CDATA[<html>Hey!! What do you think you're doing?</html>]]></talk_answer>
      <talk_answer link_index="n1131_lunaria-3-c" mid="1389" select_type="0"><![CDATA[<html>(Is it edible?)</html>]]></talk_answer>
