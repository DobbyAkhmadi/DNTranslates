   <talk_paragraph index="n701_oldkarakule-1-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-1-f" mid="1080" select_type="2" />
      <talk_question mid="1023"><![CDATA[<html>$Karacule Fairystar$<BR>Hmm... this was written in a code that is often used in Fairystar. What kind of sorceress would use such easy codes? This... is about an experiment.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n701_oldkarakule-1-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-1-g" mid="1081" select_type="2" />
