   <talk_paragraph index="n1207_illusion-1-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Illusion.dds" center_image="">
      <talk_answer link_index="n1207_illusion-1-d" mid="25" select_type="2" />
      <talk_question mid="24"><![CDATA[<html>$Illusion$<BR>I've already told you that woman is deceiving you.<BR>I've warned you time and again.</html>]]></talk_question>
