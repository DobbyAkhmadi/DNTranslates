   </talk_paragraph>
   <talk_paragraph index="n462_sidel-4-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sidel.dds" center_image="">
      <talk_answer link_index="n462_sidel-4-f" mid="140" select_type="2" />
