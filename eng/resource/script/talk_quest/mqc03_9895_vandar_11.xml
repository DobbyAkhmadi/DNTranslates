   <talk_paragraph index="n2808_hwaran-4-b" image="QN_hongryun_pether.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2808_hwaran-4-c" mid="647" select_type="2" />
      <talk_question mid="584"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>You know of him. <BR>He seems to be acquainted with Ultima of the Blue Clan. Do you know who he is?</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2808_hwaran-4-c" image="QN_DarkRubinat.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_hwaran.dds" center_image="">
      <talk_answer link_index="n2808_hwaran-4-d" mid="648" select_type="2" />
