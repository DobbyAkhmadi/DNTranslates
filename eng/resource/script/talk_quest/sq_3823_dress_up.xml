      <talk_answer link_index="n098_warrior_master_lodrigo-accepting-acceptted" mid="24" select_type="0"><![CDATA[<html><FONT color=#086171>(Accept quest)</FONT></html>]]></talk_answer>
      <talk_question mid="23"><![CDATA[<html>$How to Jump Over the Wall$</html>]]></talk_question>
