   <talk_paragraph index="n012_sorceress_master_cynthia-2" image="QS_Default.dds" itemindex="300059" portrait_left="" portrait_right="NPC_Right_SoceressCynthia.dds" center_image="">
      <talk_answer link_index="n012_sorceress_master_cynthia-2-" mid="105" select_type="5" />
      <talk_question mid="49"><![CDATA[<html>$Master Sorceress Cynthia$<BR>Be prepared to face the consequences if something happens to the <FONT color=#a82424>Vision Orb</FONT>!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n014_cleric_cedric-2" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Cl_Fallen_Cedric.dds" center_image="">
