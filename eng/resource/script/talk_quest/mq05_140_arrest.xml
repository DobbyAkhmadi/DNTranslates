   <talk_paragraph index="n015_sorceress_tara-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_SoceressTara.dds" center_image="">
      <talk_answer link_index="n015_sorceress_tara-1-a" mid="95" select_type="2" />
      <talk_question mid="6"><![CDATA[<html>$Sorceress Tara$<BR>You don't have time to rest! You must get the Orb back before Cynthia finds out! Do you remember the monster that stole the Orb?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n015_sorceress_tara-1-a" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_SoceressTara.dds" center_image="">
      <talk_answer link_index="n015_sorceress_tara-1-b" mid="96" select_type="2" />
      <talk_question mid="8"><![CDATA[<html>$Sorceress Tara$<BR>A cow? Phew… It must be a minotaur.<BR>
Luckily I know where he is. Count yourself lucky to have me by your side. </html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n015_sorceress_tara-1-b" image="QW_9.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_SoceressTara.dds" center_image="">
      <talk_answer link_index="n015_sorceress_tara-1-" mid="98" select_type="5" />
