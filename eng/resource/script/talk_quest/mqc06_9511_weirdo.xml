   <talk_paragraph index="n1691_eltia_lotus-1-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1691_eltia_lotus-1-b" mid="68" select_type="2" />
      <talk_question mid="7"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>It went well! Eltia, you really are a genius! <BR>(Tell Eltia everything related to </FONT><FONT color=#662500>Ignacio's ritual</FONT><FONT color=#662500>)</FONT></html>]]></talk_question>
