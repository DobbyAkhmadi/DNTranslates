   <talk_paragraph index="n2130_refugee_boy-1-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2130_refugee_boy-1-b" mid="45" select_type="0"><![CDATA[<html>Where?</html>]]></talk_answer>
      <talk_question mid="8"><![CDATA[<html>$Refugee Boy$<BR>Yeah! I saw that weird <FONT color=#a94407>golden creature</FONT> again!</html>]]></talk_question>
