   <talk_paragraph index="n2132_geraint-2-i" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2132_geraint-2-" mid="78" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="77"><![CDATA[<html>$Geraint$<BR>Anyways! I'm a busy man so I gotta run along!<BR>Tell that boy I'm waiting to meet him!</html>]]></talk_question>
