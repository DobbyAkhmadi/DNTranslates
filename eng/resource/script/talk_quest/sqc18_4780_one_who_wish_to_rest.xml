   <talk_paragraph index="n2043_traveler_free-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2043_traveler_free-accepting-b" mid="53" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="6"><![CDATA[<html>$Traveller Free$<BR>*Sobs* This is such a sad story. I can't stop crying… AHH! <FONT color=#387206>{user_nick}</FONT>?!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2043_traveler_free-accepting-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2043_traveler_free-accepting-c" mid="54" select_type="0"><![CDATA[<html>I didn't say anything yet…</html>]]></talk_answer>
      <talk_question mid="7"><![CDATA[<html>$Traveller Free$<BR>W-why are you giving me your hankie? I didn't cry!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2043_traveler_free-accepting-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
