   <talk_paragraph index="n1880_guard_seef-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1880_guard_seef-accepting-b" mid="8" select_type="0"><![CDATA[<html>Er... just recently? Haha...</html>]]></talk_answer>
      <talk_question mid="7"><![CDATA[<html>$Guard Chiffe$<BR>Hmm? Hey you! <FONT color=#387206>{user_nick}</FONT>? When did you come back to Merca?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1880_guard_seef-accepting-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1880_guard_seef-accepting-c" mid="10" select_type="0"><![CDATA[<html>What research?</html>]]></talk_answer>
      <talk_question mid="9"><![CDATA[<html>$Guard Chiffe$<BR>I see. Well, you're here at the right moment. Come and hear about the results of my research.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1880_guard_seef-accepting-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1880_guard_seef-accepting-acceptted" mid="12" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest </FONT></html>]]></talk_answer>
