   <talk_paragraph index="n714_cleric_yohan-accepting" image="QN_LM_Nativeman_B.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n714_cleric_yohan-accepting-a" mid="6" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Master Cleric Johan${action:Talk}<BR>I want to help the people you met in <FONT color=#a82424>Isla Native Village</FONT>.<BR>They are even injured.</html>]]></talk_question>
