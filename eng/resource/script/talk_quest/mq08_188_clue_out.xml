   <talk_paragraph index="n024_adventurer_guildmaster_decud-1-f" image="QM_dragonSycophant.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Master_Decud.dds" center_image="">
      <talk_answer link_index="n024_adventurer_guildmaster_decud-1-g" mid="326" select_type="2" />
      <talk_question mid="325"><![CDATA[<html>$Adventurer's Guild Master Deckard$<BR>Dragon Followers are a group of people who <FONT color=#a82424>worship the dragons</FONT>. Their belief is that submitting to the dragons will bring true salvation. To me, it's all a big load of bullshit.</html>]]></talk_question>
   </talk_paragraph>
