   <talk_paragraph index="n092_trader_kelly-2" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n092_trader_kelly-2-a" mid="54" select_type="0"><![CDATA[<html>(Explain what happened)</html>]]></talk_answer>
      <talk_question mid="52"><![CDATA[<html>$Merchant Kelly$<BR>How was it?</html>]]></talk_question>
