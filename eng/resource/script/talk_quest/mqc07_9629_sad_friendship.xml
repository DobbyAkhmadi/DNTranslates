   <talk_paragraph index="n701_oldkarakule-4-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-4-c" mid="825" select_type="2" />
      <talk_question mid="771"><![CDATA[<html>$Karacule Fairystar$<BR>I see…<BR>That's how she died…(Coughs)<BR><FONT color=#7e12d2>(Karacule's cough gets worse)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n701_oldkarakule-4-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-4-d" mid="826" select_type="2" />
