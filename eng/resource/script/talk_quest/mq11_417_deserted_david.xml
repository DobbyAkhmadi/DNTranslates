   <talk_paragraph index="n261_slave_david-3-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_David.dds" center_image="">
      <talk_answer link_index="n261_slave_david-3-b" mid="133" select_type="2" />
      <talk_question mid="116"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Huh? What are you doing here?</FONT></html>]]></talk_question>
