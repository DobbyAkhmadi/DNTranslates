   <talk_paragraph index="n2084_iris-1" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_iris.dds" center_image="">
      <talk_answer link_index="n2084_iris-1-" mid="117" select_type="5" />
      <talk_question mid="46"><![CDATA[<html>$Iris$<BR>Vulcanus Swamp is so fun!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2096_guard_wounded-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Adventure_Wilber.dds" center_image="">
      <talk_answer link_index="n2096_guard_wounded-1-" mid="118" select_type="5" />
