   <talk_paragraph index="n046_cleric_master_enoc-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n046_cleric_master_enoc-accepting-b" mid="9" select_type="0"><![CDATA[<html>(The lady of the family...is the step-mother...?)</html>]]></talk_answer>
      <talk_question mid="8"><![CDATA[<html>$Master Cleric Enoch${action:Talk}<BR>Um... the lady of your family... has sent the letter.<BR>Here is it. You can read it.<BR><FONT color=#7e12d2>(Enoch gives you a letter)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n046_cleric_master_enoc-accepting-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n046_cleric_master_enoc-accepting-c" mid="11" select_type="0"><![CDATA[<html>...Okay.</html>]]></talk_answer>
      <talk_question mid="10"><![CDATA[<html>$Master Cleric Enoch$<BR>It is supposed to end everything... I don't know why she sent a letter.<BR>She said to give it to you secretly, I will pretend that I did not see it. </html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n046_cleric_master_enoc-accepting-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n046_cleric_master_enoc-accepting-acceptted" mid="13" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
