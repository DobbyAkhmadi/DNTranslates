   <talk_paragraph index="n707_sidel-accepting" image="QN_Npc_karahan.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n707_sidel-accepting-a" mid="53" select_type="0"><![CDATA[<html>(Cidel seems to have decided on something)</html>]]></talk_answer>
      <talk_question mid="52"><![CDATA[<html>$Cidel$<BR>Karahan is Fairystar's disgrace.<BR>We will bring an end to him with our own hands.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n707_sidel-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n707_sidel-accepting-acceptted" mid="55" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
