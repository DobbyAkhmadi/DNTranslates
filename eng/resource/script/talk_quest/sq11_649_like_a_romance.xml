   <journal_data recompense="0:6490" quest_title="영원한 사랑처럼" chapterid="" quest_level="38" quest_mark_type="10" use_item_list="300365">
      <journal index="1">
         <contents_image>QN_Sonya.dds</contents_image>
         <destination_pos>11,n92</destination_pos>
