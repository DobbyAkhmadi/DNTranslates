   <talk_paragraph index="n2106_velskud-2" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Velskud_Human.dds" center_image="">
      <talk_answer link_index="n2106_velskud-2-a" mid="79" select_type="2" />
      <talk_question mid="21"><![CDATA[<html>$Velskud$<BR>What are you doing here? </html>]]></talk_question>
