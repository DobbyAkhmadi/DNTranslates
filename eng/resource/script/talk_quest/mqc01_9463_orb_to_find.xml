   <talk_paragraph index="n1686_eltia-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1686_eltia-1-" mid="118" select_type="5" />
      <talk_question mid="28"><![CDATA[<html>$Eltia$<BR>I hope the staff is recovered soon so that they don't accuse you anymore… Have you talked to <FONT color=#a82424>Master Cleric Leonard</FONT>?</html>]]></talk_question>
