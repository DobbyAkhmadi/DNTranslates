   <talk_paragraph index="n011_priest_leonardo-1-c" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Cl_Leonardo.dds" center_image="">
      <talk_answer link_index="n011_priest_leonardo-1-d" mid="104" select_type="2" />
