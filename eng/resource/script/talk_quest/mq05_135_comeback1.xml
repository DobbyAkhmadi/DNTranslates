   <talk_paragraph index="n015_sorceress_tara-2-c" image="QN_Npc_Karakule_silhouette.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_SoceressTara.dds" center_image="">
      <talk_answer link_index="n015_sorceress_tara-2-d" mid="181" select_type="2" />
      <talk_question mid="22"><![CDATA[<html>$Sorceress Tara$<BR>What in the world have you been doing! What happened? Where were you? Did you go to see Master <FONT color=#a82424>Karacule</FONT>?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n015_sorceress_tara-2-d" image="QN_Npc_Karakule_silhouette.dds" itemindex="" portrait_left="" portrait_right="Npc_Right_SoceressTara.dds" center_image="">
