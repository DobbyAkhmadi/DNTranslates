   <talk_paragraph index="n2650_hongryun_trader-1-d" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2650_hongryun_trader-1-e" mid="64" select_type="2" />
      <talk_question mid="14"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Blood Flower? What’s that?</FONT></html>]]></talk_question>
