   <talk_paragraph index="n762_karahan-1-b" image="QN_Npc_Charty.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_karahan.dds" center_image="">
      <talk_answer link_index="n762_karahan-1-c" mid="19" select_type="2" />
