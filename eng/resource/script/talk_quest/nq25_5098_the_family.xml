   <talk_paragraph index="n100_event_ilyn-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n100_event_ilyn-accepting-b" mid="99" select_type="0"><![CDATA[<html>I guess you are right.</html>]]></talk_answer>
