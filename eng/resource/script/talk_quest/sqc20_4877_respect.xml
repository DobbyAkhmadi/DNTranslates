   <talk_paragraph index="n2110_farghani-3-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2110_farghani-3-b" mid="43" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
