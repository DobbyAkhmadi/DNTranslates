   <talk_paragraph index="n1897_shaolong-accepting" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Shaolong.dds" center_image="">
      <talk_answer link_index="n1897_shaolong-accepting-acceptted" mid="71" select_type="3" />
      <talk_question mid="70"><![CDATA[<html>$Xiaolong$<BR><FONT color=#387206>{user_nick}</FONT>! There you are.</html>]]></talk_question>
