   <talk_paragraph index="n1121_lunaria-3-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1121_lunaria-3-b" mid="716" select_type="2" />
