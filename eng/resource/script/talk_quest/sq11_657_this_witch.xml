   <talk_paragraph index="n046_cleric_master_enoc-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n046_cleric_master_enoc-accepting-a" mid="7" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="6"><![CDATA[<html>$Master Cleric Enoch$<BR>The king, called a tyrant was excommunicated by the church.<BR>It worries me that there are still some people who are looking into that case.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n046_cleric_master_enoc-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n046_cleric_master_enoc-accepting-acceptted" mid="13" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
