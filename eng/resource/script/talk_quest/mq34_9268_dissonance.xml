   <talk_paragraph index="n1055_elf_guard_basha-accepting" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Basha.dds" center_image="">
      <talk_answer link_index="n1055_elf_guard_basha-accepting-acceptted" mid="6" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Elf Guard Basha$<BR>Then did something happen to Agelia?<BR>She must have explored the Golden Meadow at least twice by now.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1055_elf_guard_basha-1" image="QN_Npc_Azelia.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Basha.dds" center_image="">
      <talk_answer link_index="n1055_elf_guard_basha-1-a" mid="8" select_type="2" />
      <talk_question mid="7"><![CDATA[<html>$Elf Guard Basha$<BR><FONT color=#387206>Agelia</FONT> is one of the candidates for the next Queen...<BR>
I have a duty to protect her!<BR>I should go and look for her myself.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1055_elf_guard_basha-1-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Basha.dds" center_image="">
      <talk_answer link_index="n1055_elf_guard_basha-1-b" mid="10" select_type="2" />
      <talk_question mid="9"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I will look for her.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1055_elf_guard_basha-1-b" image="QW_78.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Basha.dds" center_image="">
      <talk_answer link_index="n1055_elf_guard_basha-1-" mid="12" select_type="5" />
      <talk_question mid="11"><![CDATA[<html>$Elf Guard Basha$<BR>Would you? Phew… That's a relief.<BR>Actually <FONT color=#a82424>Golden Meadow</FONT> is an area swarming with monsters so I was a little scared.<BR>
Thank you for volunteering to go.</html>]]></talk_question>
   </talk_paragraph>
