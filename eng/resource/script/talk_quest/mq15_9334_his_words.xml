   <talk_paragraph index="n701_oldkarakule-1" image="QN_Npc_Charty.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-1-a" mid="15" select_type="2" />
      <talk_question mid="14"><![CDATA[<html>$Karacule Fairystar$<BR>Why are you so busy?<BR>Cidel must have asked you to go and see that girl.</html>]]></talk_question>
