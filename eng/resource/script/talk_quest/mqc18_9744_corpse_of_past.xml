   <talk_paragraph index="n2032_merca_grele-accepting" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_child_grele.dds" center_image="">
      <talk_answer link_index="n2032_merca_grele-accepting-acceptted_1" mid="83" select_type="3" />
      <talk_question mid="5"><![CDATA[<html>$Grele$<BR>You'll help me, won't you? <FONT color=#387206>{user_nick}</FONT>?</html>]]></talk_question>
