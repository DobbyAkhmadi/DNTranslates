   <talk_paragraph index="n1046_slave_david-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1046_slave_david-accepting-a" mid="6" select_type="0"><![CDATA[<html>Again? What's up this time?</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$David$<BR>It's all because of you… <BR><FONT color=#7e12d2>(David is sobbing.)</FONT></html>]]></talk_question>
