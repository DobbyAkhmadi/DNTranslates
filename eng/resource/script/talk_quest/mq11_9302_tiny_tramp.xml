   <talk_paragraph index="n039_bishop_ignasio-2" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Bishop.dds" center_image="">
      <talk_answer link_index="n039_bishop_ignasio-2-a" mid="172" select_type="2" />
      <talk_question mid="171"><![CDATA[<html>$Bishop Ignacio$<BR>You're back. Haven't you met the other adventurers?<BR>They left not long ago.</html>]]></talk_question>
