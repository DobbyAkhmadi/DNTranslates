   <talk_paragraph index="n1139_lunaria-1-f" image="QN_Npc_Bishop.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1139_lunaria-1-" mid="168" select_type="5" />
