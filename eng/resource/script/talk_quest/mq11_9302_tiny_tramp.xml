   <talk_paragraph index="n1139_lunaria-2" image="QN_Npc_Bishop.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1139_lunaria-2-" mid="170" select_type="5" />
