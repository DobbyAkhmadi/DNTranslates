   <talk_paragraph index="n1139_lunaria-1" image="QN_Npc_Velskud.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1139_lunaria-1-a" mid="152" select_type="2" />
