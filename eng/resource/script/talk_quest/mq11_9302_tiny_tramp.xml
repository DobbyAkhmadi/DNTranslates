   <talk_paragraph index="n1144_lunaria-6" image="QN_Npc_Genaral_Duglas.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Lunaria.dds" center_image="">
      <talk_answer link_index="n1144_lunaria-6-" mid="235" select_type="5" />
