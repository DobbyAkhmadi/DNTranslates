   <talk_paragraph index="n100_event_ilyn-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n100_event_ilyn-accepting-b" mid="14" select_type="0"><![CDATA[<html>Yes! I'll do my best!</html>]]></talk_answer>
      <talk_answer link_index="n100_event_ilyn-accepting-b" mid="15" select_type="0"><![CDATA[<html>Yeah.</html>]]></talk_answer>
      <talk_answer link_index="n100_event_ilyn-accepting-b" mid="20" select_type="0"><![CDATA[<html>How can I express my deepest feelings for you?</html>]]></talk_answer>
