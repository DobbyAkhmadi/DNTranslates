   <talk_paragraph index="n775_cleric_ilizer-accepting-b" image="" itemindex="" portrait_left="" portrait_right="NPC_Left_cleric_ilizer.dds" center_image="">
      <talk_answer link_index="n775_cleric_ilizer-accepting-" mid="144" select_type="5" />
