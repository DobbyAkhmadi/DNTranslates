   <talk_paragraph index="n2882_kanna_merca-5-b" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2882_kanna_merca-5-!next" mid="210" select_type="2" />
      <talk_question mid="209"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>How did it go? What did you say?</FONT></html>]]></talk_question>
