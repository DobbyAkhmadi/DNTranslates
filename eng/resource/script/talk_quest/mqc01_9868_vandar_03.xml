   <talk_paragraph index="n2898_peep_point-5" image="QW_Merca_Heart.dds" itemindex="" portrait_left="" portrait_right="NPC__Right_Kanna.dds" center_image="">
      <talk_answer link_index="n2898_peep_point-5-" mid="223" select_type="5" />
