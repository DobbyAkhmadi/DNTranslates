   </talk_paragraph>
   <talk_paragraph index="n837_rambert-2-g" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Warrior_Lambert.dds" center_image="">
      <talk_answer link_index="n837_rambert-2-h" mid="91" select_type="2" />
