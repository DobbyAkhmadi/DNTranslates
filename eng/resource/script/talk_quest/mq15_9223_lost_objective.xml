   <talk_paragraph index="n837_rambert-2-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Warrior_Lambert.dds" center_image="">
      <talk_answer link_index="n837_rambert-2-g" mid="89" select_type="2" />
      <talk_question mid="14"><![CDATA[<html>$Rambert$<BR>
