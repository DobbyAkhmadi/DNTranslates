   <talk_paragraph index="n2043_traveler_free-1-c" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2043_traveler_free-1-d" mid="18" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="17"><![CDATA[<html>$Order of Peace and Chicken$<BR><FONT color=#086171>But too many brothers and sisters are settling for ordinary chicken bred in ordinary farms. </FONT>This is <FONT color=#a94407>unacceptable</FONT><FONT color=#086171>.</FONT></html>]]></talk_question>
