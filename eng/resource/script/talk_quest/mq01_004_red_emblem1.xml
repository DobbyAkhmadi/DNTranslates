   <talk_paragraph index="n131_guard_timose-4" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n131_guard_timose-4-a" mid="108" select_type="0"><![CDATA[<html>But… Where is Rose…?</html>]]></talk_answer>
