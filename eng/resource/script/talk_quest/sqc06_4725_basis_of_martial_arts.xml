   <talk_paragraph index="n1897_shaolong-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1897_shaolong-accepting-a" mid="6" select_type="0"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="5"><![CDATA[<html>$Xiaolong$<BR>Oh no, I should reduce the difficulty level of enlightenment.<BR>There used to be a hermit master in Lotus Marsh who distanced himself from the worldly life.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1897_shaolong-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1897_shaolong-accepting-acceptted" mid="8" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
