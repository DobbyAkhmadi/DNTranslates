   <talk_paragraph index="n1880_guard_seef-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n1880_guard_seef-accepting-acceptted" mid="40" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
