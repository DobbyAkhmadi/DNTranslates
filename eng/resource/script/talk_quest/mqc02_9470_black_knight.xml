   <talk_paragraph index="n1690_eltia-2-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1690_eltia-2-d" mid="36" select_type="2" />
      <talk_question mid="35"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>You always pout when you're unhappy. I'm sorry!!! I'm here now!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n1690_eltia-2-d" image="QN_Rose.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1690_eltia-2-e" mid="38" select_type="2" />
      <talk_question mid="37"><![CDATA[<html>$Eltia$<BR>I know you care about me.<BR>It's just that I got a little lonely being far away from home all alone. But I made friends with May over there. I'm fine now.<BR>Did you find the girl?</html>]]></talk_question>
