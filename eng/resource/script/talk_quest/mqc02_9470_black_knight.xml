   <talk_paragraph index="n1690_eltia-2-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Eltia.dds" center_image="">
      <talk_answer link_index="n1690_eltia-2-b" mid="32" select_type="2" />
      <talk_question mid="31"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Eltia, are you angry?</FONT></html>]]></talk_question>
