      <talk_answer link_index="n394_academic_station-4-d" mid="164" select_type="2" />
      <talk_question mid="163"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Are there any other investigators who are still alive?</FONT></html>]]></talk_question>
