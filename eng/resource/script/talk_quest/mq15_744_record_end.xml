   <talk_paragraph index="n394_academic_station-4-d" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Jasmin.dds" center_image="">
      <talk_answer link_index="n394_academic_station-4-e" mid="165" select_type="2" />
      <talk_question mid="87"><![CDATA[<html>$Academic Jasmine$<BR>You were the last one to leave to the past, so there are 4 more left.<BR>But they won't be disposable ones like you.<BR>After a long discussion, our plan has been finalised.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n394_academic_station-4-e" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Jasmin.dds" center_image="">
      <talk_answer link_index="n394_academic_station-4-f" mid="166" select_type="2" />
      <talk_question mid="89"><![CDATA[<html>$Academic Jasmine$<BR>Honestly, I wasn't comfortable with this plan either, but this is the last option we have.<BR>We don't have any more time to waste.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n394_academic_station-4-f" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Jasmin.dds" center_image="">
      <talk_answer link_index="n394_academic_station-4-g" mid="168" select_type="2" />
