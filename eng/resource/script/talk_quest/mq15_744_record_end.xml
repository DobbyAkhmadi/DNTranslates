   <talk_paragraph index="n394_academic_station-4-k" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Jasmin.dds" center_image="">
      <talk_answer link_index="n394_academic_station-4-l" mid="175" select_type="2" />
      <talk_question mid="174"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I will stop you and save this world. Watch me.</FONT></html>]]></talk_question>
