   <talk_paragraph index="n2255_grele-2-g" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_child_grele.dds" center_image="">
      <talk_answer link_index="n2255_grele-3" mid="46" select_type="2" />
      <talk_question mid="45"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Phew... Don't worry, I won't leave you behind.<BR>Let's go to the cave where Geraint is now.</FONT></html>]]></talk_question>
