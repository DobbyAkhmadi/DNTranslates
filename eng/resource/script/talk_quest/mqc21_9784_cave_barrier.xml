   <talk_paragraph index="n2255_grele-2-f" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_child_grele.dds" center_image="">
      <talk_answer link_index="n2255_grele-2-g" mid="44" select_type="2" />
      <talk_question mid="43"><![CDATA[<html>$Grele$<BR>Yeah... I can walk.<BR>You're not going to go without me, are you? I'm really fine now.</html>]]></talk_question>
