   <talk_paragraph index="n849_daisy-3" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n849_daisy-3-a" mid="34" select_type="0"><![CDATA[<html>(Say he wasn't there)</html>]]></talk_answer>
