   <talk_paragraph index="n485_overhear-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_karahan.dds" center_image="">
      <talk_answer link_index="n485_overhear-1-" mid="396" select_type="5" />
      <talk_question mid="317"><![CDATA[<html>$Karahan$<BR>Ah... you found me.<BR>Oh well, my shining beauty stands out in this barbaric landscape. I understand. Huhu...</html>]]></talk_question>
