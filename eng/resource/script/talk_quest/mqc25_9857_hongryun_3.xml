   <talk_paragraph index="n2815_salica-1-m" image="" itemindex="" portrait_left="Npc_Left_child_grele.dds" portrait_right="Npc_Right_Salica.dds" center_image="">
      <talk_answer link_index="n2815_salica-1-n" mid="65" select_type="2" />
