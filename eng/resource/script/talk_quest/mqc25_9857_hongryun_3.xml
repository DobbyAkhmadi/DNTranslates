   <talk_paragraph index="n2815_salica-1-l" image="" itemindex="" portrait_left="Npc_Left_child_grele.dds" portrait_right="" center_image="">
      <talk_answer link_index="n2815_salica-1-m" mid="64" select_type="2" />
      <talk_question mid="20"><![CDATA[<html>$Grele${sound:npc_GustDragon_Grele_qu9857_sp_01.ogg}<BR>Lica!<BR>Did you finish your work in Aergal Kingdom?<BR>You'll be traveling with us from now on, right?</html>]]></talk_question>
