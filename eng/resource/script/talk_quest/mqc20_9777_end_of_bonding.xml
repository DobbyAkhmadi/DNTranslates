   <talk_paragraph index="n2062_hologram_nophee-1-s" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Hologram.dds" center_image="">
      <talk_answer link_index="n2062_hologram_nophee-1-t" mid="121" select_type="2" />
