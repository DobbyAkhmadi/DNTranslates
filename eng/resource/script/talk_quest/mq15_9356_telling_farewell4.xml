   <talk_paragraph index="n713_soceress_tamara-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Lotus_SoceressTamara.dds" center_image="">
      <talk_answer link_index="n713_soceress_tamara-1-a" mid="333" select_type="2" />
      <talk_question mid="271"><![CDATA[<html>$Master Sorceress Tamara$<BR>You've come to escort me.<BR>Thank you for all your hard work, <FONT color=#387206>{user_nick}</FONT>.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n713_soceress_tamara-1-a" image="QN_Npc_karahan.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Lotus_SoceressTamara.dds" center_image="">
      <talk_answer link_index="n713_soceress_tamara-2" mid="334" select_type="2" />
      <talk_question mid="273"><![CDATA[<html>$Master Sorceress Tamara$<BR>After the funeral, the master sorceresses are going to discuss on what to do with the traitor Karahan.<BR>I hope you can continue to help us.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n801_sorceress_tessa-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Sorceress_Tessa.dds" center_image="">
