      <talk_answer link_index="n750_night_kasius-4-" mid="134" select_type="5" />
      <talk_question mid="133"><![CDATA[<html><FONT color=#7e12d2>(Nothing can be heard anymore.)</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n735_darklair_mocha-5" image="QN_Npc_Lotus_Elder_Teramai.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Darklair_Castia.dds" center_image="">
