   <talk_paragraph index="n044_archer_master_ishilien-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n044_archer_master_ishilien-accepting-1" mid="12" select_type="0"><![CDATA[<html><FONT color=#2b2723>Accept Quest</FONT></html>]]></talk_answer>
