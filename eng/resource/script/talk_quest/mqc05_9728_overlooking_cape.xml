   <talk_paragraph index="n042_general_duglars-1-c" image="QS_Default.dds" itemindex="300338" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-d" mid="104" select_type="2" />
      <talk_question mid="9"><![CDATA[<html>$???$<BR><FONT color=#7e12d2>(There's a piece of paper covered in blood. It's crumpled and it's written with words you don't understand.)</FONT></html>]]></talk_question>
