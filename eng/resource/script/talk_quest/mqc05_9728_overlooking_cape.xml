   <talk_paragraph index="n042_general_duglars-1-d" image="QS_Default.dds" itemindex="300338" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-e" mid="105" select_type="2" />
      <talk_question mid="10"><![CDATA[<html>$Bloody Note$<BR><FONT color=#7e12d2>The cape where those who waited stared at the seas.<BR>There lies the guide to the holy island.<BR>Follow the path he opens and take a step closer to the great overlord.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-1-e" image="QM_dragonSycophant.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-f" mid="106" select_type="2" />
      <talk_question mid="11"><![CDATA[<html>$General Douglas$<BR><FONT color=#7e12d2>(General Douglas reads the bloody note over and over again)<BR></FONT>Hmm… So the Black Knight has something to do with the <FONT color=#a82424>Dragon Followers</FONT>…?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-1-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-g" mid="107" select_type="2" />
      <talk_question mid="12"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>I don't think so.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n042_general_duglars-1-g" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Duglars.dds" center_image="">
      <talk_answer link_index="n042_general_duglars-1-h" mid="108" select_type="2" />
      <talk_question mid="13"><![CDATA[<html>$General Douglas$<BR>What makes you so sure?</html>]]></talk_question>
