   <talk_paragraph index="n011_priest_leonardo-3-b" image="QS_Default.dds" itemindex="300028" portrait_left="" portrait_right="Npc_Right_Cl_Leonardo.dds" center_image="">
      <talk_answer link_index="n011_priest_leonardo-3-c" mid="130" select_type="2" />
      <talk_question mid="29"><![CDATA[<html>$Master Cleric Leonard$<BR>That was frustrating. Anyway, I suppose I owe you one for recovering the Sage's Staff. Heh, how ironic that the staff was stolen by a Cleric, and recovered by a sorceress...</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n011_priest_leonardo-3-c" image="QS_Default.dds" itemindex="300028" portrait_left="" portrait_right="Npc_Right_Cl_Leonardo.dds" center_image="">
      <talk_answer link_index="n011_priest_leonardo-3-d" mid="132" select_type="2" />
      <talk_question mid="131"><![CDATA[<html>${user_nick}$<BR>
<FONT color=#662500>Oh! Cynthia had a message for you!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n011_priest_leonardo-3-d" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Cl_Leonardo.dds" center_image="">
      <talk_answer link_index="n011_priest_leonardo-3-cutscene_3" mid="133" select_type="2" />
