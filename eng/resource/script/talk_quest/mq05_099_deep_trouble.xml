      <talk_answer link_index="n391_academic_station-2-a" mid="84" select_type="2" />
      <talk_question mid="24"><![CDATA[<html>$Academic Jasmine${sound:npc_MR_Jasmin_sp_04.ogg}<BR>Good to see you, <FONT color=#387206>{user_nick}</FONT>.<BR>I was worried because I didn't hear from you for some time.</html>]]></talk_question>
