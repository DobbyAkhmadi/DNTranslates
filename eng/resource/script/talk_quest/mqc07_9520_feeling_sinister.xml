   <talk_paragraph index="n728_karahan-3-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_karahan.dds" center_image="">
      <talk_answer link_index="n728_karahan-3-b" mid="65" select_type="2" />
      <talk_question mid="18"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>You super handsome yet super weird man!! What have you done to them?!</FONT></html>]]></talk_question>
