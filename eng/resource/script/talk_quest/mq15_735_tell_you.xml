   <talk_paragraph index="n793_edan-1" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Edan.dds" center_image="">
      <talk_answer link_index="n793_edan-1-a" mid="90" select_type="2" />
      <talk_question mid="12"><![CDATA[<html>$Cleric Edan$<BR><FONT color=#387206>{user_nick}, you were here.</FONT> Did you drop this earlier, by any chance? It looks like one of your strange toys.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n793_edan-1-a" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Edan.dds" center_image="">
      <talk_answer link_index="n793_edan-1-b" mid="92" select_type="2" />
