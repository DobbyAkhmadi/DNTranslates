   <talk_paragraph index="n394_academic_station-2-e" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_Jasmin.dds" center_image="">
      <talk_answer link_index="n394_academic_station-2-f" mid="113" select_type="2" />
      <talk_question mid="70"><![CDATA[<html>$Academic Jasmine$<BR>It must be that investigator who stole your communicator. I need to look into this... In the meantime, try to stay out of trouble!</html>]]></talk_question>
