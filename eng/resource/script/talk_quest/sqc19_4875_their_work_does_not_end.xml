   <talk_paragraph index="n2043_traveler_free-1-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2043_traveler_free-1-b" mid="14" select_type="0"><![CDATA[<html>I'm sure he's dead...</html>]]></talk_answer>
      <talk_question mid="13"><![CDATA[<html>$Traveller Free$<BR>I don't know why but he's back.</html>]]></talk_question>
