   <talk_paragraph index="n040_king_casius-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n040_king_casius-accepting-a" mid="10" select_type="0"><![CDATA[<html><FONT color=#387206>{user_nick} </FONT><FONT color=#2b2723>Greetings.</FONT></html>]]></talk_answer>
