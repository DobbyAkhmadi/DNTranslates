   <talk_paragraph index="n040_king_casius-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n040_king_casius-accepting-b" mid="12" select_type="0"><![CDATA[<html>(You cant even remember...)</html>]]></talk_answer>
