   <talk_paragraph index="n2110_farghani-accepting" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2110_farghani-accepting-a" mid="591" select_type="0"><![CDATA[<html>I think I get what you're trying to say. But…</html>]]></talk_answer>
      <talk_question mid="558"><![CDATA[<html>$Astronomer Fargani$<BR>…Anyway, you might think I'm staring at the sky for nothing but I'm doing my job!</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n2110_farghani-accepting-a" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n2110_farghani-accepting-acceptted" mid="592" select_type="0"><![CDATA[<html><FONT color=#086171>Accept Quest</FONT></html>]]></talk_answer>
