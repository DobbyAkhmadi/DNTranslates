   <talk_paragraph index="n2295_grugiru-2-a" image="" itemindex="" portrait_left="" portrait_right="Npc_Right_grugiru.dds" center_image="">
      <talk_answer link_index="n2295_grugiru-2-b" mid="44" select_type="2" />
