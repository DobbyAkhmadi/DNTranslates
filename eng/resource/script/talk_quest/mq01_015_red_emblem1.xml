   <talk_paragraph index="n004_guard_steave-2" image="QN_Npc_Warrior_Dunken.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Guard_Steave.dds" center_image="">
      <talk_answer link_index="n004_guard_steave-2-" mid="237" select_type="5" />
      <talk_question mid="58"><![CDATA[<html>$Vigilante Leader Steve$<BR>Thank you. Before you go, please meet <FONT color=#a82424>Master Warrior Duncan</FONT> from the <FONT color=#a82424>Adventurer's Guild</FONT>. He would like to have a word with you,<FONT color=#387206>{user_nick}</FONT> .</html>]]></talk_question>
