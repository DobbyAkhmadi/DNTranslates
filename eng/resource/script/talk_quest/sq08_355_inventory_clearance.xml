   <talk_paragraph index="n323_warehouse_lupert-1-a" image="QN_Npc_Guilian.dds" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n323_warehouse_lupert-1-b" mid="36" select_type="0"><![CDATA[<html>Excuse me, can you at least listen to me?</html>]]></talk_answer>
