   <journal_data recompense="95920" quest_title="로제를 구할 이유 " chapterid="4" quest_level="24" quest_mark_type="1" use_item_list="">
      <journal index="1">
         <contents_image>QN_Npc_Bishop.dds</contents_image>
         <destination_pos>13,n1864</destination_pos>
         <destination_mode>custom:</destination_mode>
         <statemark_npcid>1864</statemark_npcid>
         <statemark_type>2</statemark_type>
