   <talk_paragraph index="n520_angelica-1-a" image="QW_32.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Angelica.dds" center_image="">
      <talk_answer link_index="n520_angelica-2" mid="572" select_type="2" />
      <talk_question mid="571"><![CDATA[<html>$Sorceress Angelica$<BR>Hmm? You want to know what we need to find? Hehehe. I'll take care of that.<BR>All you have to do is to kill <FONT color=#a82424>{monster:913}</FONT>.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n520_angelica-2" image="QW_32.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Angelica.dds" center_image="">
      <talk_answer link_index="n520_angelica-2-" mid="576" select_type="5" />
