   <talk_paragraph index="n520_angelica-1" image="QW_32.dds" itemindex="" portrait_left="" portrait_right="NPC_Right_Angelica.dds" center_image="">
      <talk_answer link_index="n520_angelica-1-a" mid="570" select_type="2" />
