      <talk_answer link_index="n713_soceress_tamara-2-b" mid="107" select_type="2" />
      <talk_question mid="48"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>No, I came to help you!<BR>(Tell her what you heard from Eltia)</FONT></html>]]></talk_question>
