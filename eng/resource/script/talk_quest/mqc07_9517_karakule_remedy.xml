   <talk_paragraph index="n701_oldkarakule-3-c" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-3-d" mid="125" select_type="3" />
      <talk_question mid="65"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Are you feeling better now?</FONT></html>]]></talk_question>
