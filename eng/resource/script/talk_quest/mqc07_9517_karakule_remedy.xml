   <talk_paragraph index="n701_oldkarakule-3-d" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-3-e" mid="126" select_type="2" />
      <talk_question mid="66"><![CDATA[<html>$Karacule Fairystar$<BR>Coughs won't kill me.<BR><FONT color=#7e12d2>(Karacule frowns)</FONT><BR>But what is this taste…? I told Tamara not to include extra stuff in the medicine time and again…</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n701_oldkarakule-3-e" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-3-f" mid="127" select_type="2" />
      <talk_question mid="67"><![CDATA[<html>${user_nick}$<BR><FONT color=#662500>Then you must have included a bunch of weird ingredients in your medicine too.<BR>It tasted like rotten grass!</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n701_oldkarakule-3-f" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_OldKarakule.dds" center_image="">
      <talk_answer link_index="n701_oldkarakule-3-g" mid="128" select_type="2" />
